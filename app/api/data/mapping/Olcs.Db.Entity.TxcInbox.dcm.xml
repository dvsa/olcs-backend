<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\TxcInbox" table="txc_inbox">
    <indexes>
      <index name="fk_txc_inbox_bus_reg1_idx" columns="bus_reg_id"/>
      <index name="fk_txc_inbox_local_authority1_idx" columns="local_authority_id"/>
      <index name="fk_txc_inbox_organisation1_idx" columns="organisation_id"/>
      <index name="fk_txc_inbox_document1_idx" columns="zip_document_id"/>
      <index name="fk_txc_inbox_document2_idx" columns="route_document_id"/>
      <index name="fk_txc_inbox_document3_idx" columns="pdf_document_id"/>
      <index name="fk_txc_inbox_user1_idx" columns="created_by"/>
      <index name="fk_txc_inbox_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fileRead" type="boolean" column="file_read" nullable="true"/>
    <field name="routeSeq" type="integer" column="route_seq" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="pdfDocument" target-entity="Olcs\Db\Entity\Document">
      <join-columns>
        <join-column name="pdf_document_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="routeDocument" target-entity="Olcs\Db\Entity\Document">
      <join-columns>
        <join-column name="route_document_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="zipDocument" target-entity="Olcs\Db\Entity\Document">
      <join-columns>
        <join-column name="zip_document_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="localAuthority" target-entity="Olcs\Db\Entity\LocalAuthority">
      <join-columns>
        <join-column name="local_authority_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="organisation" target-entity="Olcs\Db\Entity\Organisation">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="busReg" target-entity="Olcs\Db\Entity\BusReg">
      <join-columns>
        <join-column name="bus_reg_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
