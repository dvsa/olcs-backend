<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Note" table="note">
    <indexes>
      <index name="fk_note_application1_idx" columns="application_id"/>
      <index name="fk_note_licence1_idx" columns="licence_id"/>
      <index name="fk_note_case1_idx" columns="case_id"/>
      <index name="fk_note_irfo_gv_permit1_idx" columns="irfo_gv_permit_id"/>
      <index name="fk_note_irfo_psv_auth1_idx" columns="irfo_psv_auth_id"/>
      <index name="fk_note_user1_idx" columns="created_by"/>
      <index name="fk_note_user2_idx" columns="last_modified_by"/>
      <index name="fk_note_ref_data1_idx" columns="note_type"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="comment" type="string" column="comment" length="4000" nullable="false"/>
    <field name="priority" type="boolean" column="priority" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="noteType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="note_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoPsvAuth" target-entity="Olcs\Db\Entity\IrfoPsvAuth">
      <join-columns>
        <join-column name="irfo_psv_auth_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoGvPermit" target-entity="Olcs\Db\Entity\IrfoGvPermit">
      <join-columns>
        <join-column name="irfo_gv_permit_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
