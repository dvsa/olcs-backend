      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="piReason" target-entity="Olcs\Db\Entity\PiReason">
      <join-columns>
        <join-column name="pi_reason_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="legacyRecommendation" target-entity="Olcs\Db\Entity\LegacyRecommendation">
      <join-columns>
        <join-column name="legacy_recommendation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
