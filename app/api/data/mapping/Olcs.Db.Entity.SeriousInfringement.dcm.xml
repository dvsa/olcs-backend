<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\SeriousInfringement" table="serious_infringement">
    <indexes>
      <index name="fk_serious_infringement_cases1_idx" columns="case_id"/>
      <index name="fk_serious_infringement_user1_idx" columns="erru_response_user_id"/>
      <index name="fk_serious_infringement_country1_idx" columns="member_state_code"/>
      <index name="fk_serious_infringement_si_category1_idx" columns="si_category_id"/>
      <index name="fk_serious_infringement_si_category_type1_idx" columns="si_category_type_id"/>
      <index name="fk_serious_infringement_user2_idx" columns="created_by"/>
      <index name="fk_serious_infringement_user3_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="checkDate" type="date" column="check_date" nullable="true"/>
    <field name="erruResponseSent" type="boolean" column="erru_response_sent" nullable="false"/>
    <field name="erruResponseTime" type="datetime" column="erru_response_time" nullable="true"/>
    <field name="infringementDate" type="date" column="infringement_date" nullable="true"/>
    <field name="notificationNumber" type="string" column="notification_number" length="36" nullable="true"/>
    <field name="reason" type="string" column="reason" length="500" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="siCategoryType" target-entity="Olcs\Db\Entity\SiCategoryType">
      <join-columns>
        <join-column name="si_category_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="siCategory" target-entity="Olcs\Db\Entity\SiCategory">
      <join-columns>
        <join-column name="si_category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="erruResponseUser" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="erru_response_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="memberStateCode" target-entity="Olcs\Db\Entity\Country">
      <join-columns>
        <join-column name="member_state_code" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
