<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\PrintQueue" table="print_queue">
    <indexes>
      <index name="fk_print_queue_team_printer1_idx" columns="team_printer_id"/>
      <index name="fk_print_queue_document1_idx" columns="document_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="addedDatetime" type="datetime" column="added_datetime" nullable="true"/>
    <many-to-one field="document" target-entity="Olcs\Db\Entity\Document">
      <join-columns>
        <join-column name="document_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="teamPrinter" target-entity="Olcs\Db\Entity\TeamPrinter">
      <join-columns>
        <join-column name="team_printer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
