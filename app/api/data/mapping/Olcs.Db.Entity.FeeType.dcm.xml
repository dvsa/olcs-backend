<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\FeeType" table="fee_type">
    <indexes>
      <index name="fk_fee_type_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_fee_type_ref_data1_idx" columns="licence_type"/>
      <index name="fk_fee_type_ref_data2_idx" columns="goods_or_psv"/>
      <index name="fk_fee_type_user1_idx" columns="created_by"/>
      <index name="fk_fee_type_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="feeType" type="string" column="fee_type" length="20" nullable="false"/>
    <field name="effectiveFrom" type="date" column="effective_from" nullable="false"/>
    <field name="description" type="string" column="description" length="255" nullable="false"/>
    <field name="fixedValue" type="decimal" column="fixed_value" precision="10" scale="2" nullable="true"/>
    <field name="annualValue" type="decimal" column="annual_value" precision="10" scale="2" nullable="true"/>
    <field name="fiveYearValue" type="decimal" column="five_year_value" precision="10" scale="2" nullable="true"/>
    <field name="expireFeeWithLicence" type="boolean" column="expire_fee_with_licence" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="goodsOrPsv" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="goods_or_psv" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licenceType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="licence_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
