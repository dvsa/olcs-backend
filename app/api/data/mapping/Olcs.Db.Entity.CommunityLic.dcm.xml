<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\CommunityLic" table="community_lic">
    <indexes>
      <index name="fk_community_lic_licence1_idx" columns="licence_id"/>
      <index name="fk_community_lic_user1_idx" columns="created_by"/>
      <index name="fk_community_lic_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="expiredDate" type="date" column="expired_date" nullable="true"/>
    <field name="specifiedDate" type="datetime" column="specified_date" nullable="true"/>
    <field name="licenceExpiredDate" type="date" column="licence_expired_date" nullable="true"/>
    <field name="issueNo" type="integer" column="issue_no" nullable="true"/>
    <field name="serialNo" type="integer" column="serial_no" nullable="true"/>
    <field name="serialNoPrefix" type="string" column="serial_no_prefix" length="4" nullable="true"/>
    <field name="status" type="string" column="status" length="10" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
