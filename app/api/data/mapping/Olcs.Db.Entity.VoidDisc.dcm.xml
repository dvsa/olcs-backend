<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\VoidDisc" table="void_disc">
    <indexes>
      <index name="fk_void_disc_ref_data1_idx" columns="goods_or_psv"/>
      <index name="fk_void_disc_ref_data2_idx" columns="licence_type"/>
      <index name="fk_void_disc_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_void_disc_user1_idx" columns="created_by"/>
      <index name="fk_void_disc_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="serialStart" type="integer" column="serial_start" nullable="true"/>
    <field name="serialEnd" type="integer" column="serial_end" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licenceType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="licence_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="goodsOrPsv" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="goods_or_psv" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
