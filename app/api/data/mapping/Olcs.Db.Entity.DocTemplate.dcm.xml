<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\DocTemplate" table="doc_template">
    <indexes>
      <index name="fk_doc_template_doc_process1_idx" columns="doc_process_id"/>
      <index name="fk_doc_template_document_sub_category1_idx" columns="document_sub_category_id"/>
      <index name="fk_doc_template_document1_idx" columns="document_id"/>
      <index name="fk_doc_template_user1_idx" columns="created_by"/>
      <index name="fk_doc_template_user2_idx" columns="last_modified_by"/>
      <index name="fk_doc_template_document_category1_idx" columns="category_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="255" nullable="false"/>
    <field name="isNi" type="boolean" column="is_ni" nullable="false"/>
    <field name="suppressFromOp" type="boolean" column="suppress_from_op" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="category" target-entity="Olcs\Db\Entity\Category">
      <join-columns>
        <join-column name="category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="document" target-entity="Olcs\Db\Entity\Document">
      <join-columns>
        <join-column name="document_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="documentSubCategory" target-entity="Olcs\Db\Entity\DocumentSubCategory">
      <join-columns>
        <join-column name="document_sub_category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="docProcess" target-entity="Olcs\Db\Entity\DocProcess">
      <join-columns>
        <join-column name="doc_process_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
