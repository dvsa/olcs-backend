<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\FeePayment" table="fee_payment">
    <indexes>
      <index name="fk_fee_has_payment_payment1_idx" columns="payment_id"/>
      <index name="fk_fee_has_payment_fee1_idx" columns="fee_id"/>
      <index name="fk_fee_payment_user1_idx" columns="created_by"/>
      <index name="fk_fee_payment_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="fee" association-key="true"/>
    <id name="payment" association-key="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <one-to-one field="fee" target-entity="Olcs\Db\Entity\Fee">
      <join-columns>
        <join-column name="fee_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="payment" target-entity="Olcs\Db\Entity\Payment">
      <join-columns>
        <join-column name="payment_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
