<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\IrfoVehicle" table="irfo_vehicle">
    <indexes>
      <index name="fk_irfo_vehicle_irfo_psv_auth1_idx" columns="irfo_psv_auth_id"/>
      <index name="fk_irfo_vehicle_user1_idx" columns="created_by"/>
      <index name="fk_irfo_vehicle_user2_idx" columns="last_modified_by"/>
      <index name="fk_irfo_vehicle_irfo_gv_permit1_idx" columns="irfo_gv_permit_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cocA" type="boolean" column="coc_a" nullable="false"/>
    <field name="cocB" type="boolean" column="coc_b" nullable="false"/>
    <field name="cocC" type="boolean" column="coc_c" nullable="false"/>
    <field name="cocD" type="boolean" column="coc_d" nullable="false"/>
    <field name="cocT" type="boolean" column="coc_t" nullable="false"/>
    <field name="vrm" type="string" column="vrm" length="20" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="true"/>
    <many-to-one field="irfoGvPermit" target-entity="Olcs\Db\Entity\IrfoGvPermit">
      <join-columns>
        <join-column name="irfo_gv_permit_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoPsvAuth" target-entity="Olcs\Db\Entity\IrfoPsvAuth">
      <join-columns>
        <join-column name="irfo_psv_auth_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
