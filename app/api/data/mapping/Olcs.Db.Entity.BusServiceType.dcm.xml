<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\BusServiceType" table="bus_service_type">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="70" nullable="true"/>
    <field name="txcServiceTypeName" type="string" column="txc_service_type_name" length="70" nullable="true"/>
    <many-to-many field="busReg" target-entity="Olcs\Db\Entity\BusReg" inversed-by="busServiceType">
      <join-table name="bus_reg_bus_service_type">
        <join-columns>
          <join-column name="bus_service_type_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="bus_reg_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
