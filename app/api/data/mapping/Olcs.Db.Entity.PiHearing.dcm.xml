<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\PiHearing" table="pi_hearing">
    <indexes>
      <index name="IDX_83AFD387E0DEB379" columns="pi_id"/>
      <index name="IDX_83AFD387DE12AB56" columns="created_by"/>
      <index name="IDX_83AFD38765CF370E" columns="last_modified_by"/>
      <index name="IDX_83AFD38753BAD7A2" columns="presiding_tc_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="isAdjourned" type="boolean" column="is_adjourned" nullable="false"/>
    <field name="hearingDate" type="datetime" column="hearing_date" nullable="true"/>
    <field name="venue" type="string" column="venue" length="100" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="pi" target-entity="Olcs\Db\Entity\Pi">
      <join-columns>
        <join-column name="pi_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="presidingTc" target-entity="Olcs\Db\Entity\PresidingTc">
      <join-columns>
        <join-column name="presiding_tc_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
