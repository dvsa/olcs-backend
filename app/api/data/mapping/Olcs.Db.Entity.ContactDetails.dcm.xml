<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\ContactDetails" table="contact_details">
    <indexes>
      <index name="fk_contact_details_licence1_idx" columns="licence_id"/>
      <index name="fk_contact_details_organisation1_idx" columns="organisation_id"/>
      <index name="fk_contact_details_person1_idx" columns="person_id"/>
      <index name="fk_contact_details_address1_idx" columns="address_id"/>
      <index name="fk_contact_details_user1_idx" columns="created_by"/>
      <index name="fk_contact_details_user2_idx" columns="last_modified_by"/>
      <index name="fk_contact_details_ref_data1_idx" columns="contact_type"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="emailAddress" type="string" column="email_address" length="60" nullable="true"/>
    <field name="fao" type="string" column="fao" length="90" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="forename" type="string" column="forename" length="40" nullable="true"/>
    <field name="familyName" type="string" column="family_name" length="40" nullable="true"/>
    <field name="writtenPermissionToEngage" type="boolean" column="written_permission_to_engage" nullable="false"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="contactType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="contact_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="address" target-entity="Olcs\Db\Entity\Address">
      <join-columns>
        <join-column name="address_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="organisation" target-entity="Olcs\Db\Entity\Organisation">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="person" target-entity="Olcs\Db\Entity\Person">
      <join-columns>
        <join-column name="person_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
