      <index name="fk_TrafficAreaVehicleInspectorate_VehicleInspectorate1_idx" columns="enforcement_area_id"/>
      <index name="fk_traffic_area_enforcement_area_user1_idx" columns="created_by"/>
      <index name="fk_traffic_area_enforcement_area_user2_idx" columns="last_modified_by"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="traffic_area_id" columns="traffic_area_id,enforcement_area_id"/>
