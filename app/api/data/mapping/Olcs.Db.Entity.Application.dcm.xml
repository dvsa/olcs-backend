<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Application" table="application">
    <indexes>
      <index name="fk_application_licence1_idx" columns="licence_id"/>
      <index name="fk_application_user1_idx" columns="created_by"/>
      <index name="fk_application_user2_idx" columns="last_modified_by"/>
      <index name="fk_application_ref_data1_idx" columns="licence_type"/>
      <index name="fk_application_ref_data2_idx" columns="status"/>
      <index name="fk_application_ref_data3_idx" columns="interim_status"/>
      <index name="fk_application_ref_data4_idx" columns="withdrawn_reason"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="totAuthTrailers" type="integer" column="tot_auth_trailers" nullable="true"/>
    <field name="totAuthVehicles" type="integer" column="tot_auth_vehicles" nullable="true"/>
    <field name="totAuthSmallVehicles" type="integer" column="tot_auth_small_vehicles" nullable="true"/>
    <field name="totAuthMediumVehicles" type="integer" column="tot_auth_medium_vehicles" nullable="true"/>
    <field name="totAuthLargeVehicles" type="integer" column="tot_auth_large_vehicles" nullable="true"/>
    <field name="totCommunityLicences" type="integer" column="tot_community_licences" nullable="true"/>
    <field name="bankrupt" type="boolean" column="bankrupt" nullable="false"/>
    <field name="administration" type="boolean" column="administration" nullable="false"/>
    <field name="disqualified" type="boolean" column="disqualified" nullable="false"/>
    <field name="liquidation" type="boolean" column="liquidation" nullable="false"/>
    <field name="receivership" type="boolean" column="receivership" nullable="false"/>
    <field name="insolvencyConfirmation" type="boolean" column="insolvency_confirmation" nullable="false"/>
    <field name="insolvencyDetails" type="string" column="insolvency_details" length="4000" nullable="true"/>
    <field name="safetyConfirmation" type="boolean" column="safety_confirmation" nullable="false"/>
    <field name="receivedDate" type="datetime" column="received_date" nullable="true"/>
    <field name="targetCompletionDate" type="datetime" column="target_completion_date" nullable="true"/>
    <field name="grantedDate" type="datetime" column="granted_date" nullable="true"/>
    <field name="refusedDate" type="datetime" column="refused_date" nullable="true"/>
    <field name="withdrawnDate" type="datetime" column="withdrawn_date" nullable="true"/>
    <field name="prevHasLicence" type="boolean" column="prev_has_licence" nullable="true"/>
    <field name="prevHadLicence" type="boolean" column="prev_had_licence" nullable="true"/>
    <field name="prevBeenDisqualifiedEu" type="boolean" column="prev_been_disqualified_eu" nullable="true"/>
    <field name="prevBeenRevoked" type="boolean" column="prev_been_revoked" nullable="true"/>
    <field name="prevBeenAtPi" type="boolean" column="prev_been_at_pi" nullable="true"/>
    <field name="prevBeenDisqualifiedTc" type="boolean" column="prev_been_disqualified_tc" nullable="true"/>
    <field name="prevPurchasedAssets" type="boolean" column="prev_purchased_assets" nullable="true"/>
    <field name="overrideOoo" type="boolean" column="override_ooo" nullable="false"/>
    <field name="prevConviction" type="boolean" column="prev_conviction" nullable="false"/>
    <field name="convictionsConfirmation" type="boolean" column="convictions_confirmation" nullable="false"/>
    <field name="psvOperateSmallVhl" type="boolean" column="psv_operate_small_vhl" nullable="true"/>
    <field name="psvSmallVhlNotes" type="string" column="psv_small_vhl_notes" length="4000" nullable="true"/>
    <field name="psvSmallVhlConfirmation" type="boolean" column="psv_small_vhl_confirmation" nullable="false"/>
    <field name="psvNoSmallVhlConfirmation" type="boolean" column="psv_no_small_vhl_confirmation" nullable="false"/>
    <field name="psvLimosines" type="boolean" column="psv_limosines" nullable="false"/>
    <field name="psvNoLimosineConfirmation" type="boolean" column="psv_no_limosine_confirmation" nullable="false"/>
    <field name="psvOnlyLimosinesConfirmation" type="boolean" column="psv_only_limosines_confirmation" nullable="false"/>
    <field name="interimStart" type="date" column="interim_start" nullable="true"/>
    <field name="interimEnd" type="date" column="interim_end" nullable="true"/>
    <field name="interimAuthVehicles" type="integer" column="interim_auth_vehicles" nullable="true"/>
    <field name="interimAuthTrailers" type="integer" column="interim_auth_trailers" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="interimStatus" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="interim_status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="withdrawnReason" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="withdrawn_reason" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="status" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licenceType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="licence_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="applicationActionRef" target-entity="Olcs\Db\Entity\ApplicationActionRef" inversed-by="application">
      <join-table name="application_action">
        <join-columns>
          <join-column name="application_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="application_action_ref_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
