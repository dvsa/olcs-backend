<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\TmCaseDecisionUnfitness" table="tm_case_decision_unfitness">
    <indexes>
      <index name="fk_tm_case_decision_unfitness_tm_case_decision1_idx" columns="tm_case_decision_id"/>
      <index name="fk_tm_case_decision_unfitness_ref_data1_idx" columns="unfitness_reason_id"/>
      <index name="fk_tm_case_decision_unfitness_user1_idx" columns="created_by"/>
      <index name="fk_tm_case_decision_unfitness_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="unfitnessReason" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="unfitness_reason_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tmCaseDecision" target-entity="Olcs\Db\Entity\TmCaseDecision">
      <join-columns>
        <join-column name="tm_case_decision_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
