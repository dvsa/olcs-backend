<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Cases" table="cases">
    <indexes>
      <index name="fk_case_application1_idx" columns="application_id"/>
      <index name="fk_case_licence1_idx" columns="licence_id"/>
      <index name="fk_case_user1_idx" columns="created_by"/>
      <index name="fk_case_user2_idx" columns="last_modified_by"/>
      <index name="fk_cases_transport_manager1_idx" columns="transport_manager_id"/>
      <index name="fk_cases_ref_data1_idx" columns="case_type"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ecmsNo" type="string" column="ecms_no" length="45" nullable="true"/>
    <field name="openDate" type="date" column="open_date" nullable="true"/>
    <field name="closeDate" type="date" column="close_date" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="description" type="string" column="description" length="1024" nullable="true"/>
    <field name="isImpounding" type="boolean" column="is_impounding" nullable="false"/>
    <field name="erruOriginatingAuthority" type="string" column="erru_originating_authority" length="50" nullable="true"/>
    <field name="erruTransportUndertakingName" type="string" column="erru_transport_undertaking_name" length="100" nullable="true"/>
    <field name="erruVrm" type="string" column="erru_vrm" length="15" nullable="true"/>
    <field name="annualTestHistory" type="string" column="annual_test_history" length="4000" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="transportManager" target-entity="Olcs\Db\Entity\TransportManager">
      <join-columns>
        <join-column name="transport_manager_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="caseType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="case_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="legacyOffence" target-entity="Olcs\Db\Entity\LegacyOffence" inversed-by="case">
      <join-table name="legacy_case_offence">
        <join-columns>
          <join-column name="case_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="legacy_offence_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="submissionSection" target-entity="Olcs\Db\Entity\SubmissionSection" inversed-by="case">
      <join-table name="case_submission_section">
        <join-columns>
          <join-column name="case_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="submission_section_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
