<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\TmQualification" table="tm_qualification">
    <indexes>
      <index name="fk_qualification_transport_manager1_idx" columns="transport_manager_id"/>
      <index name="fk_qualification_country1_idx" columns="country_code"/>
      <index name="fk_qualification_ref_data1_idx" columns="qualification_type"/>
      <index name="fk_tm_qualification_user1_idx" columns="created_by"/>
      <index name="fk_tm_qualification_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="issuedDate" type="date" column="issued_date" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="serialNo" type="string" column="serial_no" length="20" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="qualificationType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="qualification_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="countryCode" target-entity="Olcs\Db\Entity\Country">
      <join-columns>
        <join-column name="country_code" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="transportManager" target-entity="Olcs\Db\Entity\TransportManager">
      <join-columns>
        <join-column name="transport_manager_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
