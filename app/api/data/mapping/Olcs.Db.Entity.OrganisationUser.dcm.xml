<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\OrganisationUser" table="organisation_user">
    <indexes>
      <index name="fk_organisation_has_user_user1_idx" columns="user_id"/>
      <index name="fk_organisation_has_user_organisation1_idx" columns="organisation_id"/>
      <index name="fk_organisation_user_user1_idx" columns="created_by"/>
      <index name="fk_organisation_user_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="organisation" association-key="true"/>
    <id name="user" association-key="true"/>
    <field name="isAdministrator" type="boolean" column="is_administrator" nullable="false"/>
    <field name="addedDate" type="datetime" column="added_date" nullable="true"/>
    <field name="removedDate" type="datetime" column="removed_date" nullable="true"/>
    <field name="sftpAccess" type="boolean" column="sftp_access" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <one-to-one field="organisation" target-entity="Olcs\Db\Entity\Organisation">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="user" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
