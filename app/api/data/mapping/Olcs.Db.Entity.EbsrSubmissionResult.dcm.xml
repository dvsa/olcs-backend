<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\EbsrSubmissionResult" table="ebsr_submission_result">
    <id name="id" type="string" column="id" length="64">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="emailSubject" type="string" column="email_subject" length="45" nullable="true"/>
    <field name="emailBodyTemplate" type="string" column="email_body_template" length="255" nullable="true"/>
    <field name="emailAuthority" type="boolean" column="email_authority" nullable="false"/>
    <field name="description" type="string" column="description" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
