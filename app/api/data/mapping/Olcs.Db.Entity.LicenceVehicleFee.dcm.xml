<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\LicenceVehicleFee" table="licence_vehicle_fee">
    <indexes>
      <index name="fk_licence_vehicle_fee_fee1_idx" columns="fee_id"/>
      <index name="fk_licence_vehicle_fee_user1_idx" columns="created_by"/>
      <index name="fk_licence_vehicle_fee_user2_idx" columns="last_modified_by"/>
      <index name="fk_licence_vehicle_fee_licence_vehicle1" columns="licence_vehicle_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fee" target-entity="Olcs\Db\Entity\Fee">
      <join-columns>
        <join-column name="fee_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licenceVehicle" target-entity="Olcs\Db\Entity\LicenceVehicle">
      <join-columns>
        <join-column name="licence_vehicle_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
