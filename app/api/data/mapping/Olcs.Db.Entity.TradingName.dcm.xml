<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\TradingName" table="trading_name">
    <indexes>
      <index name="fk_trading_name_licence1_idx" columns="licence_id"/>
      <index name="fk_trading_name_organisation1_idx" columns="organisation_id"/>
      <index name="fk_trading_name_user1_idx" columns="created_by"/>
      <index name="fk_trading_name_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="160" nullable="false"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="viAction" type="string" column="vi_action" length="1" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="organisation" target-entity="Olcs\Db\Entity\Organisation">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
