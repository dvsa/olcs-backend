<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\LicenceVehicle" table="licence_vehicle">
    <indexes>
      <index name="fk_licence_vehicle_vehicle1_idx" columns="vehicle_id"/>
      <index name="fk_licence_vehicle_user1_idx" columns="created_by"/>
      <index name="fk_licence_vehicle_user2_idx" columns="last_modified_by"/>
      <index name="fk_licence_vehicle_ref_data1_idx" columns="removal_reason"/>
      <index name="fk_licence_vehicle_application1_idx" columns="application_id"/>
      <index name="fk_licence_vehicle_licence1" columns="licence_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="applicationReceivedDate" type="date" column="application_received_date" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="removal" type="boolean" column="removal" nullable="true"/>
    <field name="removalLetterSeedDate" type="datetime" column="removal_letter_seed_date" nullable="true"/>
    <field name="isInterim" type="integer" column="is_interim" nullable="true"/>
    <field name="viAction" type="string" column="vi_action" length="1" nullable="true"/>
    <field name="warningLetterSeedDate" type="datetime" column="warning_letter_seed_date" nullable="true"/>
    <field name="warningLetterSentDate" type="datetime" column="warning_letter_sent_date" nullable="true"/>
    <field name="specifiedDate" type="datetime" column="specified_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="removalReason" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="removal_reason" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="vehicle" target-entity="Olcs\Db\Entity\Vehicle">
      <join-columns>
        <join-column name="vehicle_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
