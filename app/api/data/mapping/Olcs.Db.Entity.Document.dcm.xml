<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Document" table="document">
    <indexes>
      <index name="fk_document_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_document_document_category1_idx" columns="category_id"/>
      <index name="fk_document_document_sub_category1_idx" columns="document_sub_category_id"/>
      <index name="fk_document_licence1_idx" columns="licence_id"/>
      <index name="fk_document_application1_idx" columns="application_id"/>
      <index name="fk_document_cases1_idx" columns="case_id"/>
      <index name="fk_document_transport_manager1_idx" columns="transport_manager_id"/>
      <index name="fk_document_operating_centre1_idx" columns="operating_centre_id"/>
      <index name="fk_document_user1_idx" columns="created_by"/>
      <index name="fk_document_user2_idx" columns="last_modified_by"/>
      <index name="fk_document_opposition1_idx" columns="opposition_id"/>
      <index name="fk_document_bus_reg1_idx" columns="bus_reg_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentStoreId" type="string" column="document_store_id" length="255" nullable="false"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="isReadOnly" type="boolean" column="is_read_only" nullable="true"/>
    <field name="issuedDate" type="date" column="issued_date" nullable="true"/>
    <field name="filename" type="string" column="filename" length="255" nullable="true"/>
    <field name="size" type="integer" column="size" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="operatingCentre" target-entity="Olcs\Db\Entity\OperatingCentre">
      <join-columns>
        <join-column name="operating_centre_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="opposition" target-entity="Olcs\Db\Entity\Opposition">
      <join-columns>
        <join-column name="opposition_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="busReg" target-entity="Olcs\Db\Entity\BusReg">
      <join-columns>
        <join-column name="bus_reg_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="transportManager" target-entity="Olcs\Db\Entity\TransportManager">
      <join-columns>
        <join-column name="transport_manager_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="category" target-entity="Olcs\Db\Entity\Category">
      <join-columns>
        <join-column name="category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="documentSubCategory" target-entity="Olcs\Db\Entity\DocumentSubCategory">
      <join-columns>
        <join-column name="document_sub_category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="email" target-entity="Olcs\Db\Entity\Email" mapped-by="document"/>
  </entity>
</doctrine-mapping>
