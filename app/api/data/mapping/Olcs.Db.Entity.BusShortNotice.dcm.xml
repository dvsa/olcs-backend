<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\BusShortNotice" table="bus_short_notice">
    <indexes>
      <index name="fk_bus_short_notice_user1_idx" columns="created_by"/>
      <index name="fk_bus_short_notice_user2_idx" columns="last_modified_by"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="bus_reg_id" columns="bus_reg_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bankHolidayChange" type="boolean" column="bank_holiday_change" nullable="false"/>
    <field name="unforseenChange" type="boolean" column="unforseen_change" nullable="false"/>
    <field name="unforseenDetail" type="string" column="unforseen_detail" length="255" nullable="true"/>
    <field name="timetableChange" type="boolean" column="timetable_change" nullable="false"/>
    <field name="timetableDetail" type="string" column="timetable_detail" length="255" nullable="true"/>
    <field name="replacementChange" type="boolean" column="replacement_change" nullable="false"/>
    <field name="replacementDetail" type="string" column="replacement_detail" length="255" nullable="true"/>
    <field name="holidayChange" type="boolean" column="holiday_change" nullable="false"/>
    <field name="holidayDetail" type="string" column="holiday_detail" length="255" nullable="true"/>
    <field name="trcChange" type="boolean" column="trc_change" nullable="false"/>
    <field name="trcDetail" type="string" column="trc_detail" length="255" nullable="true"/>
    <field name="policeChange" type="boolean" column="police_change" nullable="false"/>
    <field name="policeDetail" type="string" column="police_detail" length="255" nullable="true"/>
    <field name="specialOccasionChange" type="boolean" column="special_occasion_change" nullable="false"/>
    <field name="specialOccasionDetail" type="string" column="special_occasion_detail" length="255" nullable="true"/>
    <field name="connectionChange" type="boolean" column="connection_change" nullable="false"/>
    <field name="connectionDetail" type="string" column="connection_detail" length="255" nullable="true"/>
    <field name="notAvailableChange" type="boolean" column="not_available_change" nullable="false"/>
    <field name="notAvailableDetail" type="string" column="not_available_detail" length="255" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="busReg" target-entity="Olcs\Db\Entity\BusReg">
      <join-columns>
        <join-column name="bus_reg_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
