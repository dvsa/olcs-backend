<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\PhoneContact" table="phone_contact">
    <indexes>
      <index name="fk_phone_contact_contact_details1_idx" columns="contact_details_id"/>
      <index name="fk_phone_contact_ref_data1_idx" columns="phone_contact_type"/>
      <index name="fk_phone_contact_user1_idx" columns="created_by"/>
      <index name="fk_phone_contact_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="phoneNumber" type="string" column="phone_number" length="45" nullable="true"/>
    <field name="details" type="string" column="details" length="45" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="phoneContactType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="phone_contact_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="contactDetails" target-entity="Olcs\Db\Entity\ContactDetails">
      <join-columns>
        <join-column name="contact_details_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
