<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\IrfoPermitStock" table="irfo_permit_stock">
    <indexes>
      <index name="fk_irfo_permit_stock_irfo_gv_permit1_idx" columns="irfo_gv_permit_id"/>
      <index name="fk_irfo_permit_stock_irfo_country1_idx" columns="irfo_country_id"/>
      <index name="fk_irfo_permit_stock_ref_data1_idx" columns="status"/>
      <index name="fk_irfo_permit_stock_user1_idx" columns="created_by"/>
      <index name="fk_irfo_permit_stock_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="serialNo" type="integer" column="serial_no" nullable="false"/>
    <field name="validForYear" type="integer" column="valid_for_year" nullable="false"/>
    <field name="voidReturnDate" type="date" column="void_return_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="status" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoCountry" target-entity="Olcs\Db\Entity\IrfoCountry">
      <join-columns>
        <join-column name="irfo_country_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoGvPermit" target-entity="Olcs\Db\Entity\IrfoGvPermit">
      <join-columns>
        <join-column name="irfo_gv_permit_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
