<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Printer" table="printer">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="printerTray" type="string" column="printer_tray" length="45" nullable="true"/>
    <field name="printerName" type="string" column="printer_name" length="45" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
