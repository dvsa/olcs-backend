<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Pi" table="pi">
    <indexes>
      <index name="fk_pi_cases1_idx" columns="case_id"/>
      <index name="fk_pi_presiding_tc1_idx" columns="presiding_tc_id"/>
      <index name="fk_pi_ref_data1_idx" columns="presided_by"/>
      <index name="fk_pi_ref_data2_idx" columns="pi_status"/>
      <index name="fk_pi_user1_idx" columns="created_by"/>
      <index name="fk_pi_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="agreedDate" type="date" column="agreed_date" nullable="true"/>
    <field name="typeAppNew" type="boolean" column="type_app_new" nullable="false"/>
    <field name="typeAppVar" type="boolean" column="type_app_var" nullable="false"/>
    <field name="typeDisciplinary" type="boolean" column="type_disciplinary" nullable="false"/>
    <field name="typeEnvNew" type="boolean" column="type_env_new" nullable="false"/>
    <field name="typeEnvVar" type="boolean" column="type_env_var" nullable="false"/>
    <field name="typeOcReview" type="boolean" column="type_oc_review" nullable="false"/>
    <field name="typeImpounding" type="boolean" column="type_impounding" nullable="false"/>
    <field name="typeOther" type="boolean" column="type_other" nullable="false"/>
    <field name="piDatetime" type="datetime" column="pi_datetime" nullable="true"/>
    <field name="venue" type="string" column="venue" length="255" nullable="true"/>
    <field name="witnesses" type="integer" column="witnesses" nullable="true"/>
    <field name="presidingTcOther" type="string" column="presiding_tc_other" length="45" nullable="true"/>
    <field name="isCancelled" type="boolean" column="is_cancelled" nullable="false"/>
    <field name="isAdjourned" type="boolean" column="is_adjourned" nullable="false"/>
    <field name="sectionCodeText" type="string" column="section_code_text" length="1024" nullable="true"/>
    <field name="rescheduleDatetime" type="datetime" column="reschedule_datetime" nullable="true"/>
    <field name="decisionDate" type="date" column="decision_date" nullable="true"/>
    <field name="licenceRevokedAtPi" type="boolean" column="licence_revoked_at_pi" nullable="false"/>
    <field name="notificationDate" type="date" column="notification_date" nullable="true"/>
    <field name="decisionNotes" type="text" column="decision_notes" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="piStatus" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="pi_status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="presidedBy" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="presided_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="presidingTc" target-entity="Olcs\Db\Entity\PresidingTc">
      <join-columns>
        <join-column name="presiding_tc_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
