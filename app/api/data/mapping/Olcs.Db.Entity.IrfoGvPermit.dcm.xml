<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\IrfoGvPermit" table="irfo_gv_permit">
    <indexes>
      <index name="fk_irfo_gv_permit_user1_idx" columns="created_by"/>
      <index name="fk_irfo_gv_permit_user2_idx" columns="last_modified_by"/>
      <index name="fk_irfo_gv_permit_organisation1_idx" columns="organisation_id"/>
      <index name="fk_irfo_gv_permit_irfo_gv_permit_type1_idx" columns="irfo_gv_permit_type_id"/>
      <index name="fk_irfo_gv_permit_ref_data1_idx" columns="irfo_permit_status"/>
      <index name="fk_irfo_gv_permit_ref_data2_idx" columns="withdrawn_reason"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="exemptionDetails" type="string" column="exemption_details" length="255" nullable="true"/>
    <field name="expiryDate" type="date" column="expiry_date" nullable="true"/>
    <field name="isFeeExempt" type="boolean" column="is_fee_exempt" nullable="false"/>
    <field name="inForceDate" type="date" column="in_force_date" nullable="true"/>
    <field name="irfoFeeId" type="string" column="irfo_fee_id" length="10" nullable="true"/>
    <field name="noOfCopies" type="integer" column="no_of_copies" nullable="false"/>
    <field name="note" type="string" column="note" length="2000" nullable="true"/>
    <field name="permitPrinted" type="boolean" column="permit_printed" nullable="false"/>
    <field name="yearRequired" type="integer" column="year_required" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="irfoPermitStatus" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="irfo_permit_status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="withdrawnReason" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="withdrawn_reason" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoGvPermitType" target-entity="Olcs\Db\Entity\IrfoGvPermitType">
      <join-columns>
        <join-column name="irfo_gv_permit_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="organisation" target-entity="Olcs\Db\Entity\Organisation">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
