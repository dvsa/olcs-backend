<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\CompanySubsidiaryLicence" table="company_subsidiary_licence">
    <indexes>
      <index name="fk_company_subsidiary_has_licence_licence1_idx" columns="licence_id"/>
      <index name="fk_company_subsidiary_has_licence_company_subsidiary1_idx" columns="company_subsidiary_id"/>
      <index name="fk_company_subsidiary_licence_user1_idx" columns="created_by"/>
      <index name="fk_company_subsidiary_licence_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="companySubsidiary" association-key="true"/>
    <id name="licence" association-key="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <one-to-one field="companySubsidiary" target-entity="Olcs\Db\Entity\CompanySubsidiary">
      <join-columns>
        <join-column name="company_subsidiary_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
