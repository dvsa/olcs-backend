<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\CorrespondenceInbox" table="correspondence_inbox">
    <indexes>
      <index name="fk_correspondence_inbox_document1_idx" columns="document_id"/>
      <index name="fk_correspondence_inbox_licence1_idx" columns="licence_id"/>
      <index name="fk_correspondence_inbox_user1_idx" columns="created_by"/>
      <index name="fk_correspondence_inbox_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="archived" type="boolean" column="archived" nullable="true"/>
    <field name="accessed" type="boolean" column="accessed" nullable="true"/>
    <field name="emailReminderSent" type="boolean" column="email_reminder_sent" nullable="true"/>
    <field name="printed" type="boolean" column="printed" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="document" target-entity="Olcs\Db\Entity\Document">
      <join-columns>
        <join-column name="document_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
