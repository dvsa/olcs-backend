<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\ApplicationActionRef" table="application_action_ref">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="45" nullable="false"/>
    <field name="displayOrder" type="integer" column="display_order" nullable="false"/>
    <field name="defaultReceived" type="boolean" column="default_received" nullable="true"/>
    <field name="defaultApproved" type="boolean" column="default_approved" nullable="true"/>
    <field name="defaultApplicable" type="string" column="default_applicable" length="45" nullable="true"/>
    <many-to-many field="application" target-entity="Olcs\Db\Entity\Application" mapped-by="applicationActionRef"/>
  </entity>
</doctrine-mapping>
