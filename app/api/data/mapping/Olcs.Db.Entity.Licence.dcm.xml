<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Licence" table="licence">
    <indexes>
      <index name="fk_licence_vehicle_inspectorate1_idx" columns="enforcement_area_id"/>
      <index name="fk_licence_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_licence_organisation1_idx" columns="organisation_id"/>
      <index name="fk_licence_user1_idx" columns="created_by"/>
      <index name="fk_licence_user2_idx" columns="last_modified_by"/>
      <index name="fk_licence_ref_data1_idx" columns="goods_or_psv"/>
      <index name="fk_licence_ref_data2_idx" columns="licence_type"/>
      <index name="fk_licence_ref_data3_idx" columns="status"/>
      <index name="fk_licence_ref_data4_idx" columns="tachograph_ins"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="licNo" type="string" column="lic_no" length="18" nullable="true"/>
    <field name="viAction" type="string" column="vi_action" length="1" nullable="true"/>
    <field name="totAuthTrailers" type="integer" column="tot_auth_trailers" nullable="true"/>
    <field name="totAuthVehicles" type="integer" column="tot_auth_vehicles" nullable="true"/>
    <field name="totAuthSmallVehicles" type="integer" column="tot_auth_small_vehicles" nullable="true"/>
    <field name="totAuthMediumVehicles" type="integer" column="tot_auth_medium_vehicles" nullable="true"/>
    <field name="totAuthLargeVehicles" type="integer" column="tot_auth_large_vehicles" nullable="true"/>
    <field name="totCommunityLicences" type="integer" column="tot_community_licences" nullable="true"/>
    <field name="trailersInPossession" type="integer" column="trailers_in_possession" nullable="true"/>
    <field name="fabsReference" type="string" column="fabs_reference" length="10" nullable="true"/>
    <field name="expiryDate" type="date" column="expiry_date" nullable="true"/>
    <field name="grantedDate" type="date" column="granted_date" nullable="true"/>
    <field name="reviewDate" type="date" column="review_date" nullable="true"/>
    <field name="feeDate" type="date" column="fee_date" nullable="true"/>
    <field name="inForceDate" type="date" column="in_force_date" nullable="true"/>
    <field name="surrenderedDate" type="date" column="surrendered_date" nullable="true"/>
    <field name="safetyInsTrailers" type="integer" column="safety_ins_trailers" nullable="true"/>
    <field name="safetyInsVehicles" type="integer" column="safety_ins_vehicles" nullable="true"/>
    <field name="safetyIns" type="boolean" column="safety_ins" nullable="false"/>
    <field name="safetyInsVaries" type="boolean" column="safety_ins_varies" nullable="false"/>
    <field name="niFlag" type="boolean" column="ni_flag" nullable="false"/>
    <field name="tachographInsName" type="string" column="tachograph_ins_name" length="90" nullable="true"/>
    <field name="psvDiscsToBePrintedNo" type="integer" column="psv_discs_to_be_printed_no" nullable="true"/>
    <field name="translateToWelsh" type="boolean" column="translate_to_welsh" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="licenceType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="licence_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="status" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tachographIns" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="tachograph_ins" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="goodsOrPsv" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="goods_or_psv" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="organisation" target-entity="Olcs\Db\Entity\Organisation">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="enforcementArea" target-entity="Olcs\Db\Entity\EnforcementArea">
      <join-columns>
        <join-column name="enforcement_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
