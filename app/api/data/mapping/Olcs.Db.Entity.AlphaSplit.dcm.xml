<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\AlphaSplit" table="alpha_split">
    <indexes>
      <index name="fk_alpha_split_user1_idx" columns="user_id"/>
      <index name="fk_alpha_split_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_alpha_split_user2_idx" columns="created_by"/>
      <index name="fk_alpha_split_user3_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fromInclusive" type="string" column="from_inclusive" length="2" nullable="true"/>
    <field name="toInclusive" type="string" column="to_inclusive" length="2" nullable="true"/>
    <field name="isDeleted" type="boolean" column="is_deleted" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
