<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\DiscSequence" table="disc_sequence">
    <indexes>
      <index name="fk_disc_sequence_ref_data1_idx" columns="goods_or_psv"/>
      <index name="fk_disc_sequence_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_disc_sequence_user1_idx" columns="created_by"/>
      <index name="fk_disc_sequence_user2_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="restricted" type="integer" column="restricted" nullable="true"/>
    <field name="specialRestricted" type="integer" column="special_restricted" nullable="true"/>
    <field name="standardNational" type="integer" column="standard_national" nullable="true"/>
    <field name="standardInternational" type="integer" column="standard_international" nullable="true"/>
    <field name="isSelfServe" type="boolean" column="is_self_serve" nullable="false"/>
    <field name="isNiSelfServe" type="boolean" column="is_ni_self_serve" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="goodsOrPsv" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="goods_or_psv" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
