<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Impounding" table="impounding">
    <indexes>
      <index name="fk_impounding_user1_idx" columns="created_by"/>
      <index name="fk_impounding_user2_idx" columns="last_modified_by"/>
      <index name="fk_impounding_transport_commissioner1_idx" columns="presiding_tc_id"/>
      <index name="fk_impounding_ref_data1_idx" columns="outcome"/>
      <index name="fk_impounding_ref_data2_idx" columns="impounding_type"/>
      <index name="fk_impounding_cases1_idx" columns="case_id"/>
      <index name="fk_impounding_pi_venue1_idx" columns="pi_venue_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hearingDate" type="datetime" column="hearing_date" nullable="true"/>
    <field name="applicationReceiptDate" type="datetime" column="application_receipt_date" nullable="true"/>
    <field name="outcomeSentDate" type="datetime" column="outcome_sent_date" nullable="true"/>
    <field name="notes" type="string" column="notes" length="4000" nullable="true"/>
    <field name="closeDate" type="datetime" column="close_date" nullable="true"/>
    <field name="piVenueOther" type="string" column="pi_venue_other" length="255" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="piVenue" target-entity="Olcs\Db\Entity\PiVenue">
      <join-columns>
        <join-column name="pi_venue_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="impoundingType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="impounding_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="outcome" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="outcome" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="presidingTc" target-entity="Olcs\Db\Entity\PresidingTc">
      <join-columns>
        <join-column name="presiding_tc_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
