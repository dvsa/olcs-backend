<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\EventHistory" table="event_history">
    <indexes>
      <index name="fk_event_history_event_history_type1_idx" columns="event_history_type_id"/>
      <index name="fk_event_history_user1_idx" columns="user_id"/>
      <index name="fk_event_history_licence1_idx" columns="licence_id"/>
      <index name="fk_event_history_application1_idx" columns="application_id"/>
      <index name="fk_event_history_licence_vehicle1_idx" columns="licence_vehicle_id"/>
      <index name="fk_event_history_team1_idx" columns="team_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventDatetime" type="datetime" column="event_datetime" nullable="false"/>
    <many-to-one field="licenceVehicle" target-entity="Olcs\Db\Entity\LicenceVehicle">
      <join-columns>
        <join-column name="licence_vehicle_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="team" target-entity="Olcs\Db\Entity\Team">
      <join-columns>
        <join-column name="team_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="eventHistoryType" target-entity="Olcs\Db\Entity\EventHistoryType">
      <join-columns>
        <join-column name="event_history_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
