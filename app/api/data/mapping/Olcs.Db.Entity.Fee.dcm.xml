<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Fee" table="fee">
    <indexes>
      <index name="fk_fee_application1_idx" columns="application_id"/>
      <index name="fk_fee_bus_reg1_idx" columns="bus_reg_id"/>
      <index name="fk_fee_licence1_idx" columns="licence_id"/>
      <index name="fk_fee_task1_idx" columns="task_id"/>
      <index name="fk_fee_fee_type1_idx" columns="fee_type_id"/>
      <index name="fk_fee_fee1_idx" columns="parent_fee_id"/>
      <index name="fk_fee_user1_idx" columns="waive_recommender_user_id"/>
      <index name="fk_fee_user2_idx" columns="waive_approver_user_id"/>
      <index name="fk_fee_waive_reason1_idx" columns="waive_reason_id"/>
      <index name="fk_fee_user3_idx" columns="created_by"/>
      <index name="fk_fee_user4_idx" columns="last_modified_by"/>
      <index name="fk_fee_irfo_gv_permit1_idx" columns="irfo_gv_permit_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="amount" type="decimal" column="amount" precision="10" scale="2" nullable="false"/>
    <field name="receivedAmount" type="decimal" column="received_amount" precision="10" scale="2" nullable="true"/>
    <field name="invoiceNo" type="integer" column="invoice_no" nullable="true"/>
    <field name="invoiceLineNo" type="integer" column="invoice_line_no" nullable="true"/>
    <field name="invoicedDate" type="datetime" column="invoiced_date" nullable="true"/>
    <field name="receivedDate" type="datetime" column="received_date" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="feeStatus" type="string" column="fee_status" length="20" nullable="true"/>
    <field name="waiveApprovalDate" type="datetime" column="waive_approval_date" nullable="true"/>
    <field name="waiveReason" type="string" column="waive_reason" length="255" nullable="true"/>
    <field name="waiveRecommendationDate" type="datetime" column="waive_recommendation_date" nullable="true"/>
    <field name="irfoFeeId" type="string" column="irfo_fee_id" length="10" nullable="true"/>
    <field name="irfoFeeExempt" type="boolean" column="irfo_fee_exempt" nullable="true"/>
    <field name="irfoFileNo" type="string" column="irfo_file_no" length="10" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="waiveReason2" target-entity="Olcs\Db\Entity\WaiveReason">
      <join-columns>
        <join-column name="waive_reason_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="waiveApproverUser" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="waive_approver_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="irfoGvPermit" target-entity="Olcs\Db\Entity\IrfoGvPermit">
      <join-columns>
        <join-column name="irfo_gv_permit_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="waiveRecommenderUser" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="waive_recommender_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="parentFee" target-entity="Olcs\Db\Entity\Fee">
      <join-columns>
        <join-column name="parent_fee_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="busReg" target-entity="Olcs\Db\Entity\BusReg">
      <join-columns>
        <join-column name="bus_reg_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="task" target-entity="Olcs\Db\Entity\Task">
      <join-columns>
        <join-column name="task_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="feeType" target-entity="Olcs\Db\Entity\FeeType">
      <join-columns>
        <join-column name="fee_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
