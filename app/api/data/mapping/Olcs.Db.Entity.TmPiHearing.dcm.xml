<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\TmPiHearing" table="tm_pi_hearing">
    <indexes>
      <index name="fk_tm_pi_hearing_cases1_idx" columns="case_id"/>
      <index name="fk_tm_pi_hearing_ref_data1_idx" columns="presided_by"/>
      <index name="fk_tm_pi_hearing_ref_data2_idx" columns="reason_id"/>
      <index name="fk_tm_pi_hearing_ref_data3_idx" columns="type_id"/>
      <index name="fk_tm_pi_hearing_presiding_tc1_idx" columns="presiding_tc_id"/>
      <index name="fk_tm_pi_hearing_user1_idx" columns="created_by"/>
      <index name="fk_tm_pi_hearing_user2_idx" columns="last_modified_by"/>
      <index name="fk_tm_pi_hearing_pi_venue1_idx" columns="venue_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="witnesses" type="integer" column="witnesses" nullable="false"/>
    <field name="adjournedDate" type="date" column="adjourned_date" nullable="true"/>
    <field name="agreedDate" type="date" column="agreed_date" nullable="true"/>
    <field name="cancelledDate" type="date" column="cancelled_date" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="scheduledOn" type="datetime" column="scheduled_on" nullable="true"/>
    <field name="rescheduledOn" type="datetime" column="rescheduled_on" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="venue" target-entity="Olcs\Db\Entity\PiVenue">
      <join-columns>
        <join-column name="venue_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="presidingTc" target-entity="Olcs\Db\Entity\PresidingTc">
      <join-columns>
        <join-column name="presiding_tc_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="type" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="presidedBy" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="presided_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="reason" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="reason_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
