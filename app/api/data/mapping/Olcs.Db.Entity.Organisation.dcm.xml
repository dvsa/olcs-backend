<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\Organisation" table="organisation">
    <indexes>
      <index name="fk_organisation_user1_idx" columns="created_by"/>
      <index name="fk_organisation_user2_idx" columns="last_modified_by"/>
      <index name="fk_organisation_ref_data1_idx" columns="type"/>
      <index name="fk_organisation_ref_data2_idx" columns="sic_code"/>
      <index name="fk_organisation_traffic_area1_idx" columns="lead_tc_area_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="companyOrLlpNo" type="string" column="company_or_llp_no" length="20" nullable="true"/>
    <field name="name" type="string" column="name" length="160" nullable="true"/>
    <field name="viAction" type="string" column="vi_action" length="1" nullable="true"/>
    <field name="isMlh" type="boolean" column="is_mlh" nullable="false"/>
    <field name="companyCertSeen" type="boolean" column="company_cert_seen" nullable="false"/>
    <field name="irfoNationality" type="string" column="irfo_nationality" length="45" nullable="true"/>
    <field name="isIrfo" type="boolean" column="is_irfo" nullable="false"/>
    <field name="allowEmail" type="boolean" column="allow_email" nullable="false"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="leadTcArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="lead_tc_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sicCode" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="sic_code" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="type" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
