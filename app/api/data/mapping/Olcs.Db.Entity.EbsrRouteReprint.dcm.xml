<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\EbsrRouteReprint" table="ebsr_route_reprint">
    <indexes>
      <index name="fk_ebsr_route_reprint_bus_reg1_idx" columns="bus_reg_id"/>
      <index name="fk_ebsr_route_reprint_user1_idx" columns="requested_user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="exceptionName" type="string" column="exception_name" length="45" nullable="true"/>
    <field name="scale" type="integer" column="scale" nullable="false"/>
    <field name="publishedTimestamp" type="datetime" column="published_timestamp" nullable="true"/>
    <field name="requestedTimestamp" type="datetime" column="requested_timestamp" nullable="false"/>
    <many-to-one field="requestedUser" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="requested_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="busReg" target-entity="Olcs\Db\Entity\BusReg">
      <join-columns>
        <join-column name="bus_reg_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
