<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\PublicationLink" table="publication_link">
    <indexes>
      <index name="fk_publication_has_licence_licence1_idx" columns="licence_id"/>
      <index name="fk_publication_has_licence_publication1_idx" columns="publication_id"/>
      <index name="fk_licence_publication_pi_detail1_idx" columns="pi_detail_id"/>
      <index name="fk_licence_publication_traffic_area1_idx" columns="traffic_area_id"/>
      <index name="fk_licence_publication_application1_idx" columns="application_id"/>
      <index name="fk_licence_publication_bus_reg1_idx" columns="bus_reg_id"/>
      <index name="fk_licence_publication_publication_section1_idx" columns="publication_section_id"/>
      <index name="fk_licence_publication_user1_idx" columns="created_by"/>
      <index name="fk_licence_publication_user2_idx" columns="last_modified_by"/>
      <index name="fk_licence_publication_tm_pi_hearing1_idx" columns="tm_pi_hearing_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="publicationNo" type="integer" column="publication_no" nullable="false"/>
    <field name="pubType" type="string" column="pub_type" length="3" nullable="false"/>
    <field name="sectionId" type="integer" column="section_id" nullable="true"/>
    <field name="text1" type="text" column="text1" nullable="true"/>
    <field name="text2" type="text" column="text2" nullable="true"/>
    <field name="text3" type="text" column="text3" nullable="true"/>
    <field name="origPubDate" type="date" column="orig_pub_date" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="publicationSection" target-entity="Olcs\Db\Entity\PublicationSection">
      <join-columns>
        <join-column name="publication_section_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tmPiHearing" target-entity="Olcs\Db\Entity\TmPiHearing">
      <join-columns>
        <join-column name="tm_pi_hearing_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="busReg" target-entity="Olcs\Db\Entity\BusReg">
      <join-columns>
        <join-column name="bus_reg_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="piDetail" target-entity="Olcs\Db\Entity\PiDetail">
      <join-columns>
        <join-column name="pi_detail_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="trafficArea" target-entity="Olcs\Db\Entity\TrafficArea">
      <join-columns>
        <join-column name="traffic_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="publication" target-entity="Olcs\Db\Entity\Publication">
      <join-columns>
        <join-column name="publication_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
