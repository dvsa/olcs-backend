<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\InspectionRequest" table="inspection_request">
    <indexes>
      <index name="fk_inspection_request_licence1_idx" columns="licence_id"/>
      <index name="fk_inspection_request_application1_idx" columns="application_id"/>
      <index name="fk_inspection_request_operating_centre1_idx" columns="operating_centre_id"/>
      <index name="fk_inspection_request_task1_idx" columns="task_id"/>
      <index name="fk_inspection_request_cases1_idx" columns="case_id"/>
      <index name="fk_inspection_request_ref_data1_idx" columns="report_type"/>
      <index name="fk_inspection_request_ref_data2_idx" columns="request_type"/>
      <index name="fk_inspection_request_ref_data3_idx" columns="result_type"/>
      <index name="fk_inspection_request_user1_idx" columns="requestor_user_id"/>
      <index name="fk_inspection_request_user2_idx" columns="created_by"/>
      <index name="fk_inspection_request_user3_idx" columns="last_modified_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="requestorNotes" type="text" column="requestor_notes" nullable="true"/>
    <field name="inspectorNotes" type="text" column="inspector_notes" nullable="true"/>
    <field name="dueDate" type="date" column="due_date" nullable="true"/>
    <field name="fromDate" type="date" column="from_date" nullable="true"/>
    <field name="toDate" type="date" column="to_date" nullable="true"/>
    <field name="requestDate" type="date" column="request_date" nullable="true"/>
    <field name="returnDate" type="datetime" column="return_date" nullable="true"/>
    <field name="deferredDate" type="date" column="deferred_date" nullable="true"/>
    <field name="inspectorName" type="string" column="inspector_name" length="70" nullable="true"/>
    <field name="localServicesNo" type="integer" column="local_services_no" nullable="true"/>
    <field name="trailorsExaminedNo" type="integer" column="trailors_examined_no" nullable="true"/>
    <field name="vehiclesExaminedNo" type="integer" column="vehicles_examined_no" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="resultType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="result_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="requestorUser" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="requestor_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="requestType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="request_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="reportType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="report_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="operatingCentre" target-entity="Olcs\Db\Entity\OperatingCentre">
      <join-columns>
        <join-column name="operating_centre_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="task" target-entity="Olcs\Db\Entity\Task">
      <join-columns>
        <join-column name="task_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
