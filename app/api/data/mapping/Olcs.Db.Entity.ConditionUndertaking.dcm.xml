<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\ConditionUndertaking" table="condition_undertaking">
    <indexes>
      <index name="fk_Condition_ref_data1_idx" columns="added_via"/>
      <index name="fk_Condition_ref_data2_idx" columns="attached_to"/>
      <index name="fk_Condition_ref_data3_idx" columns="condition_type"/>
      <index name="fk_Condition_cases1_idx" columns="case_id"/>
      <index name="fk_Condition_licence1_idx" columns="licence_id"/>
      <index name="fk_Condition_operating_centre1_idx" columns="operating_centre_id"/>
      <index name="fk_condition_undertaking_application1_idx" columns="application_id"/>
      <index name="fk_condition_undertaking_user1_idx" columns="created_by"/>
      <index name="fk_condition_undertaking_user2_idx" columns="last_modified_by"/>
      <index name="fk_condition_undertaking_condition_undertaking1_idx" columns="lic_condition_variation_id"/>
      <index name="fk_condition_undertaking_user3_idx" columns="approval_user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="conditionDate" type="datetime" column="condition_date" nullable="false"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="isDraft" type="boolean" column="is_draft" nullable="false"/>
    <field name="isFulfilled" type="boolean" column="is_fulfilled" nullable="false"/>
    <field name="notes" type="string" column="notes" length="8000" nullable="true"/>
    <field name="isApproved" type="boolean" column="is_approved" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licConditionVariation" target-entity="Olcs\Db\Entity\ConditionUndertaking">
      <join-columns>
        <join-column name="lic_condition_variation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="approvalUser" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="approval_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="application" target-entity="Olcs\Db\Entity\Application">
      <join-columns>
        <join-column name="application_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="operatingCentre" target-entity="Olcs\Db\Entity\OperatingCentre">
      <join-columns>
        <join-column name="operating_centre_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="attachedTo" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="attached_to" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="conditionType" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="condition_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="addedVia" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="added_via" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
