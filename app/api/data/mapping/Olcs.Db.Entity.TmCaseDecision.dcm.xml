<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\TmCaseDecision" table="tm_case_decision">
    <indexes>
      <index name="fk_tm_case_decision_ref_data1_idx" columns="decision"/>
      <index name="fk_tm_case_decision_user1_idx" columns="created_by"/>
      <index name="fk_tm_case_decision_user2_idx" columns="last_modified_by"/>
      <index name="fk_tm_case_decision_cases1_idx" columns="case_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="decisionDate" type="date" column="decision_date" nullable="true"/>
    <field name="notifiedDate" type="date" column="notified_date" nullable="true"/>
    <field name="deletedDate" type="datetime" column="deleted_date" nullable="true"/>
    <field name="isMsi" type="boolean" column="is_msi" nullable="false"/>
    <field name="reputeNotLostReason" type="string" column="repute_not_lost_reason" length="4000" nullable="true"/>
    <field name="unfitnessStartDate" type="date" column="unfitness_start_date" nullable="true"/>
    <field name="unfitnessEndDate" type="date" column="unfitness_end_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="case" target-entity="Olcs\Db\Entity\Cases">
      <join-columns>
        <join-column name="case_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="decision" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="decision" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
