<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Olcs\Db\Entity\PsvDisc" table="psv_disc">
    <indexes>
      <index name="fk_psv_disc_licence1_idx" columns="licence_id"/>
      <index name="fk_psv_disc_user1_idx" columns="created_by"/>
      <index name="fk_psv_disc_user2_idx" columns="last_modified_by"/>
      <index name="fk_psv_disc_ref_data1_idx" columns="removal_reason"/>
      <index name="fk_psv_disc_ref_data2_idx" columns="removal_explanation"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="discNo" type="string" column="disc_no" length="50" nullable="true"/>
    <field name="issuedDate" type="datetime" column="issued_date" nullable="true"/>
    <field name="ceasedDate" type="datetime" column="ceased_date" nullable="true"/>
    <field name="isCopy" type="boolean" column="is_copy" nullable="true"/>
    <field name="reprintRequired" type="boolean" column="reprint_required" nullable="true"/>
    <field name="startDate" type="date" column="start_date" nullable="true"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="true"/>
    <field name="lastModifiedOn" type="datetime" column="last_modified_on" nullable="true"/>
    <field name="version" type="integer" column="version" nullable="false"/>
    <many-to-one field="removalExplanation" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="removal_explanation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="removalReason" target-entity="Olcs\Db\Entity\RefData">
      <join-columns>
        <join-column name="removal_reason" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastModifiedBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="last_modified_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createdBy" target-entity="Olcs\Db\Entity\User">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licence" target-entity="Olcs\Db\Entity\Licence">
      <join-columns>
        <join-column name="licence_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
