<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.5" id="TXC_vehicleJourney_support">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2004-02-01</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4  Modularised from txc_common							  -->				</Date>				<Description> Common  Vehicle Journey Type element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.5/txc/}TXC_vehicleJourney_support-v2-5.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.5/txc/TXC_types-v2-3.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2015</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.5</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common  Vehicle Journey type Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_vehicleJourney_support   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="../napt/NaPT_utility_types-v2-5.xsd"/>	<!-- ======================================================================= -->	<xsd:simpleType name="VehicleJourneyCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for a particular vehicle journey.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:complexType name="VehicleJourneyRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a VehicleJourney</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="VehicleJourneyCodeType"/>		</xsd:simpleContent>	</xsd:complexType>	<xsd:simpleType name="GroupAsEnumeration">		<xsd:annotation>			<xsd:documentation>Allowed valeus for group as</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="profile"/>			<xsd:enumeration value="mondayToFriday"/>			<xsd:enumeration value="saturday"/>			<xsd:enumeration value="sunday"/>			<xsd:enumeration value="saturdayAndSunday"/>			<xsd:enumeration value="holidaysOnly"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= -->	<xsd:simpleType name="VehicleJourneyTimingLinkIdType">		<xsd:annotation>			<xsd:documentation>Type for VehicleJourney Timing Link Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="VehicleJourneyTimingLinkRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a VehicleJourney Timing Link</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="VehicleJourneyTimingLinkIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<xsd:simpleType name="VehicleJourneyStopUsageIdType">		<xsd:annotation>			<xsd:documentation>Type for Vehicle Journey Stop Usage Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType></xsd:schema>