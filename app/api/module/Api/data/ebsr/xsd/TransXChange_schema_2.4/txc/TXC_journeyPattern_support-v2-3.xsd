<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_journeyPattern_support">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2004-02-01</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4  Modularised from txc_common							  -->				</Date>				<Description> Common element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_journeyPattern_support-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_types-v2-3.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_journey-v2-3.xsd</Requires>					<Requires>http://www.w3.org/2001/xml.xsd</Requires>					<Requires>apd/AddressTypes-v1-3.xsd</Requires>				</Relation>				<Relation>					<HasPart>http://www.transxchange.org.uk/schema/2.4/txc/TXC_types.xsd</HasPart>					<isPartOf>http://www.transxchange.org.uk/schema/2.4/txc/TXC_general.xsd</isPartOf>					<isPartOf>http://www.transxchange.org.uk/schema/2.4/txc/TXC_registration.xsd</isPartOf>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tour Bus stops and stations, Transport, Public transport, Bus services, Coach services, Bus stops and stations Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_ journeyPattern_support  HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="../napt/NaPT_utility_types-v2-1.xsd"/>	<!-- ==== Journey Pattern =================================================== -->	<xsd:simpleType name="JourneyPatternIdType">		<xsd:annotation>			<xsd:documentation>Type for JourneyPattern identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="JourneyPatternRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a JourneyPattern</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="JourneyPatternIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ===== Journey Pattern Section ========================================= -->	<xsd:simpleType name="JourneyPatternSectionIdType">		<xsd:annotation>			<xsd:documentation>Type for Journey Pattern Section Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="JourneyPatternSectionRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a JourneyPattern Section</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="JourneyPatternSectionIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- == Timing/Link ==================================================== -->	<xsd:simpleType name="JourneyPatternTimingLinkIdType">		<xsd:annotation>			<xsd:documentation>Type for JourneyPattern Timing Link Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="JourneyPatternTimingLinkRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a JourneyPattern</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="JourneyPatternTimingLinkIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:simpleType name="JourneyPatternStopUsageIdType">		<xsd:annotation>			<xsd:documentation>Type for JourneyPattern StopUsage Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="StopUsageRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a JourneyPattern Section</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="JourneyPatternStopUsageIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<xsd:complexType name="JourneyPatternUsageRefStructure">		<xsd:annotation>			<xsd:documentation>Reference to a Journey Pattern and a stop within it.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="JourneyPatternRef" type="JourneyPatternRefStructure">				<xsd:annotation>					<xsd:documentation>JourneyPattern connecting at an Interchange.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="StopUsageRef" type="StopUsageRefStructure">				<xsd:annotation>					<xsd:documentation>Stop usage of stop in journey pattern at which journey interchange connects.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<!-- === Journey Pattern Interchange ============================================= -->	<xsd:simpleType name="JourneyPatternInterchangeIdType">		<xsd:annotation>			<xsd:documentation>Type for JourneyPatternInterchangeIdentifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="JourneyPatternInterchangeRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a JourneyPattern Section</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="JourneyPatternInterchangeIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:complexType name="ViasStructure">		<xsd:annotation>			<xsd:documentation>Via place name listing</xsd:documentation>		</xsd:annotation>		<xsd:choice>			<xsd:element name="Via" type="NaturalLanguageStringStructure" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Name of intermediate point to presented as Via point from this stop.  </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="None" type="EmptyType">				<xsd:annotation>					<xsd:documentation>No Vias should be shown - suppress any from parent. (TXC 2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:choice>	</xsd:complexType></xsd:schema>