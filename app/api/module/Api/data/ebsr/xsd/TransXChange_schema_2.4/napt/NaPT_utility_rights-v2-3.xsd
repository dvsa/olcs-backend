<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="NaPT_utility_rights">	<!--NaPT_utility_rights  HEADER DOCUMENTATION-->	<xsd:include schemaLocation="NaPT_utility_types-v2-1.xsd"/>	<xsd:include schemaLocation="NaPT_versioningAttributes-v2-2.xsd"/>	<xsd:import namespace="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" schemaLocation="../apd/AddressTypes-v1-3.xsd">		<xsd:annotation>			<xsd:documentation source="http://www.govtalk.gov.uk/schemasstandards/agreedschema_schema.asp?schemaid=204">A set of Address and personal details schema, including BS7666 version 1.3</xsd:documentation>		</xsd:annotation>	</xsd:import>	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Added for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.4 Nick Knowles, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2010-03-09</Created>				</Date>				<Date>					<Modified>2010-03-12</Modified>					<!-- 2.4  Added							PTIC-031 Permissions							  -->				</Date>				<Description> Common Data Right  Type element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_utility_types-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/napt/NaPT_utility_rights-v2-31.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/napt/NaPT_versioningAttributes-v2-1.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Spawned out   2.2 draft schema. Aligned with Community rights of Cyclenet exchanged</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Data right   Type  Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>==Header ==</xsd:documentation>	</xsd:annotation>	<!-- Group Definitions -->	<!--===== Note  ===============-->	<!-- ======================================================================= -->	<!-- Abstract features -->	<xsd:simpleType name="OrganisationCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for a serviced organization. In the case of LEAs and schools this should be the Department of Education Number.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:complexType name="OrganisationRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a ServicedOrganisation</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="OrganisationCodeType">				<xsd:attribute name="CodeSpace" type="xsd:normalizedString">					<xsd:annotation>						<xsd:documentation>Code space for Organization code, eg edubase, ucas, dnf. TXC v2.4</xsd:documentation>					</xsd:annotation>				</xsd:attribute>			</xsd:extension>		</xsd:simpleContent>	</xsd:complexType>	<xsd:element name="Organisation" abstract="false" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>Abstract Organisation</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="OrganisationStructure"/>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="OrganisationStructure" abstract="true">		<xsd:annotation>			<xsd:documentation>Type for an organisation referenced by a bus schedule and for which specific working days and holidays may be defined. A serviced organisation can have child serviced organisations. A child serviced organisation overrides the working days and holidays of its parent. An example parent would be all the schools in a Local Education Authority; this would define the default term days (working days) and holidays for the schools in the area. It might have child schools, in order for individual schools to override the default term days and holidays.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="OrganisationCode" type="OrganisationRefStructure">						<xsd:annotation>							<xsd:documentation>Identifying code for the serviced organisation. In the case of LEAs and Schools, this should be the Department of Education Number.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="PrivateCode" type="xsd:normalizedString" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Alternative code for legacy compatibility</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Name" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>The name of the organisation. @lang.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="OrganisationContactGroup">						<xsd:annotation>							<xsd:documentation>Contact details for Organisation. (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:group>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="OrganisationContactGroup">		<xsd:annotation>			<xsd:documentation>Contact details for Organisation</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="PostalAddress" type="apd:UKPostalAddressStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Address to be used for correspondence  (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactTelephoneNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact telephone number. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactPerson" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of contact person (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="EmailAddress" type="EmailAddressType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The email address of the organisation. It is up to the operator whether an individual's address, or a generic company e-mail address is used. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="WebSite" type="xsd:anyURI" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The web site address of the organisation (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="FurtherDetails" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Further details  (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:complexType name="ContributorsStructure">		<xsd:annotation>			<xsd:documentation>Type for list of contributors</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element ref="Contributor" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Type of organisation that is a Data right Owner</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="Contributor" abstract="false" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation> Contributor.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="ContributorStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="ContributorStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for a named of contributors.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="OrganisationStructure">				<xsd:sequence>					<xsd:group ref="ContributorGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="ContributorGroup">		<xsd:annotation>			<xsd:documentation>Elements that characterise a contributor. (TXC v2.4)</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="PolicyStatus" type="PolicyStatusEnumeration" default="SubjectToFreedomOfInformation" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Status under the Freedom Of Information Ac. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContributorClassification" type="ContributorClassificationEnumeration" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Type of Contributor. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="DataRights" type="DataRightsStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Data rights issued by contributor. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:simpleType name="ContributorClassificationEnumeration">		<xsd:annotation>			<xsd:documentation>Enumeration of contributor classifications.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="CommunityMember">				<xsd:annotation>					<xsd:documentation>Contributor is a private individual </xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ProfessionalOrganisation">				<xsd:annotation>					<xsd:documentation>Contributor is a professional Organisation</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="LocalAuthority">				<xsd:annotation>					<xsd:documentation>Contributor is a local authority.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ProfessionalOrganisationForLocalAuthority">				<xsd:annotation>					<xsd:documentation>Contributor is a professional Organisation acting on behalf of a Local Authority.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="ContributorRoleEnumeration">		<xsd:annotation>			<xsd:documentation>Enumeration of  contributor roles.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Aggregate">				<xsd:annotation>					<xsd:documentation>Contributor is a local authority.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="Distribute">				<xsd:annotation>					<xsd:documentation>Contributor is a professional Organisation acting on behalf of a Local Authority.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="Supply">				<xsd:annotation>					<xsd:documentation>Contributor is a private individual </xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="Verifiy">				<xsd:annotation>					<xsd:documentation>Contributor is a professional Organisation</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="Own"/>			<xsd:enumeration value="Update"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= -->	<xsd:complexType name="DataRightsStructure">		<xsd:annotation>			<xsd:documentation>Type for recording the data rights of an element.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element ref="DataRight" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Data right description. (TXC v2.4)@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="DataRight" substitutionGroup="VersionedChild">		<xsd:annotation>			<xsd:documentation>Data rights   (+TXC v2.4) </xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="DataRightStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="DataRightStructure">		<xsd:annotation>			<xsd:documentation>Type for recording the data rights of an element.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedChildStructure">				<xsd:sequence>					<xsd:group ref="DataRightGroup">						<xsd:annotation>							<xsd:documentation>Elements relating to community contribution. (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:group>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="DataRightGroup">		<xsd:annotation>			<xsd:documentation>Set of Data rights.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="TermsOfUse" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Terms of use appending to data (TXC v2.4)</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="AllowedUse" type="AllowedUseEnumeration" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Permission to use data. Default is open licence.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="TermsOfUseUrl" type="xsd:anyURI" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Reference to web URL with terms and conditions of use.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="TermsAndConditions" type="NaturalLanguageStringStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Local statement of terms and conditions of use.</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="Copyright" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Copyright statement identifying copyright holder (TXC v2.4)</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="CopyrightUrl" type="xsd:anyURI" minOccurs="0">							<xsd:annotation>								<xsd:documentation>URL describing Copyright</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="CopyrightStatement" type="NaturalLanguageStringStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Copyright Statement.</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="DataPolicy" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Policy  Applying to data elements. If not specififed inherit from parent element (TXC v2.4)</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="PolicyStatus" type="PolicyStatusEnumeration" default="ExemptFromFreedomOfInformation" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Policy status of element. Defualt is FOI exemptt</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="PolicyUrl" type="xsd:anyURI" minOccurs="0">							<xsd:annotation>								<xsd:documentation>URL providing information on the policy</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="PolicyJustification" type="NaturalLanguageStringStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Text explaining policy</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="VerificationStatus" type="VerificationStatusEnumeration" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Verification status of elements associated with this data right. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:simpleType name="PolicyStatusEnumeration">		<xsd:annotation>			<xsd:documentation>Enumeration of verification status of data.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="SubjectToFreedomOfInformation">				<xsd:annotation>					<xsd:documentation>Holder is Subject to the Freedom Of information Act.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ExemptFromFreedomOfInformation">				<xsd:annotation>					<xsd:documentation>Holder is not Subject to the Freedom Of information Act.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="Other">				<xsd:annotation>					<xsd:documentation>Other.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="VerificationStatusEnumeration">		<xsd:annotation>			<xsd:documentation>Enumeration of verification status of data.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Unverified">				<xsd:annotation>					<xsd:documentation>Data has not been verified.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="CommunityVerified">				<xsd:annotation>					<xsd:documentation>Data has   been verified by a community organisation.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ProfessionallyVerified">				<xsd:annotation>					<xsd:documentation>Data has   been verified by a professional organisation.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="AllowedUseEnumeration">		<xsd:annotation>			<xsd:documentation>Enumeration of allowed uses.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="CommercialLicence">				<xsd:annotation>					<xsd:documentation>Data may be used under a commercial licence.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="OpenLicence">				<xsd:annotation>					<xsd:documentation>Data  may be used under an Open Source Licence..</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="NotForProfit">				<xsd:annotation>					<xsd:documentation>Data has may be used for free under an Open Source Licence provided it is not charged for..</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="Unrestricted">				<xsd:annotation>					<xsd:documentation>Data may be used without charge, subject to terms and conditions.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= --></xsd:schema>