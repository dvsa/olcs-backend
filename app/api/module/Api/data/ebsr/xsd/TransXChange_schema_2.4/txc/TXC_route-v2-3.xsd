<?xml version="1.0" encoding="UTF-8"?> <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.42" id="TXC_route">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Modified>2010-06-10</Modified>					<!-- 2.4 						PTIC-013 Add Line, By Direction Origin, Destination, Vias Description								PTIC-041 Add Marketing Name to Line			 -->				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4 						PTIC-999 Technical changes for XML						Modularise from txc_common							Add structures for Route Link, Route Section, Feature, Track Instruction							Add id to track,  RouteIdType RouteLinkIdType, RouteSectionIdType					  	PTIC-041 Parameterise route colours for Lines 						PTIC-044 Additional Change Management Support - Deltas                                      Enhance definitions to use an explicit  versioned Object & Substitution Groups									 Add Route, RouteLink & Track elements									 Allow Change attributes on track & Line				                     Add Extensions to Line-->				</Date>				<Date>					<Created>2004-02-01</Created>				</Date>				<Description> Common Route declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_route-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_journey_support-v2-3.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Route Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_route   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ================================================================= -->	<xsd:include schemaLocation="TXC_journey_support-v2-3.xsd"/>	<!-- ===RouteSection========================================================= -->	<xsd:complexType name="RouteSectionsStructure">		<xsd:annotation>			<xsd:documentation>Geospatial representation of route sections for mapping and instructional purposes. @Modification</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="RouteSection" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- Route Section  -->	<xsd:element name="RouteSection" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>A reusable section of route comprising one or more route links, ordered in sequence of traversal.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="RouteSectionStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="RouteSectionStructure">		<xsd:annotation>			<xsd:documentation>Type for Geospatial representation of route section for mapping and instructional purposes.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element ref="RouteLink" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:simpleType name="RouteSectionIdType">		<xsd:annotation>			<xsd:documentation>Type for  Route Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="RouteSectionRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a route Section</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="RouteLinkIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ==RouteLink=========================================================== -->	<xsd:element name="RouteLink" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>A piece of the network topology connection two stops.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="RouteLinkStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="RouteLinkStructure">		<xsd:annotation>			<xsd:documentation>Type for a route link.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="ParentRouteRef" type="RouteRefStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Optional reference to parent Route. Normally not stated as given by context, but may be specified when using Link as a stand alone artefact. If already given by context, this value is ignored. (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="From" type="StopPointWrappedRefStructure">						<xsd:annotation>							<xsd:documentation>The stop at which the link begins.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="To" type="StopPointWrappedRefStructure">						<xsd:annotation>							<xsd:documentation>The stop at which the link ends.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Distance" type="DistanceType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Distance in metres along track of the link.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Direction" type="LinkDirectionEnumeration" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Direction of the route running over the link. Enumerated value.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Track" type="TrackStructure" minOccurs="0" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>A track describes a piece of the path of a route link that can be projected onto the geospatial model. The choice of how route links are divided into tracks is left to TransXChange implementors. For example, a major road junction might represent the end of one track and the beginning of the next.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:simpleType name="RouteLinkIdType">		<xsd:annotation>			<xsd:documentation>Type for Route Link Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="RouteLinkRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a route Link</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="RouteLinkIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ===Route============================================================== -->	<xsd:complexType name="RoutesStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more routes.  @Modification</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="Route" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="Route" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>A path over the transport network topology that a bus takes to deliver a service.  An ordered collection of links connecting NaPTAN stops; the links are grouped into sections.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="RouteStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="RouteStructure">		<xsd:annotation>			<xsd:documentation>Type for a Route:  Path through the physical road network which is composed of an ordered collection of track segments. The order of segments should represent the order in which they are traversed in the physical route.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="PrivateCode" type="PrivateCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>A private code that uniquely identifies the route. May be used for interoperating with other information systems.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Description" type="NaturalLanguageStringStructure">						<xsd:annotation>							<xsd:documentation>Short description of the Route. @lang.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="RouteSectionRef" type="RouteSectionIdType" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>An ordered collection of RouteSections that make up the Route. The order is the sequence of traversal.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="ReversingManoeuvres" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Present if reversing manoeuvres needed along route.</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:simpleType name="RouteIdType">		<xsd:annotation>			<xsd:documentation>Type for Route Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="RouteRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a route</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="RouteIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ==Line============================================================== -->	<xsd:element name="Line" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>A Line by which all or part of the service is known.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="LineStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="LineStructure">		<xsd:annotation>			<xsd:documentation>Type for A Line by which all or part of the service is known.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="LineName" type="NaturalLanguageStringStructure">						<xsd:annotation>							<xsd:documentation>Public identifier of the service. E.g. "N73".</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="MarketingName" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Marketing  of the service. E.g. "FastLink" (+TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="OutboundDescription" type="LineDescriptionStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Description of line Outbound (TXC+ v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="InboundDescription" type="LineDescriptionStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Description of line inbound (TXC+ v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="LineColourGroup">						<xsd:annotation>							<xsd:documentation>Elements Relating to colouring of Line   (+TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:group>					<xsd:element name="LineImage" type="xsd:anyURI" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Reference to an image associated with line.  (+TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="LineDescriptionGroup">		<xsd:annotation>			<xsd:documentation>Elements Relating to colouring of Line </xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Origin" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Origin of line for publicity purposes (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Destination" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Destination of line for publicity purposes (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Vias" minOccurs="0">				<xsd:complexType>					<xsd:sequence>						<xsd:element name="Via" type="NaturalLanguageStringStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Via point on line for publicity purposes (+TXC v2.4)</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="Description" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Description of line for publicity purposes (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="LineColourGroup">		<xsd:annotation>			<xsd:documentation>Elements Relating to colouring of Line </xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="LineColour" type="ColourValueType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Preferred Colour Value for showing line on graphics media.  (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LineFontColour" type="ColourValueType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Preferred Colour Value for presenting any text for line.  (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="AlternativeLineColour" type="ColourValueType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Alternative Colour Value for showing line on graphics media.  (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="AlternativeLineFontColour" type="ColourValueType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Preferred Colour Value for presenting any text for line.  (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:simpleType name="LineIdType">		<xsd:annotation>			<xsd:documentation>Type for Line Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="LinkRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a Line.</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="LineIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<xsd:complexType name="LineDescriptionStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a Line.(+TXC v2.4)</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Origin" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Origin of line for publicity purposes (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Destination" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Destination of line for publicity purposes (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Vias" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Via points on line</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="Via" type="NaturalLanguageStringStructure" minOccurs="0" maxOccurs="unbounded">							<xsd:annotation>								<xsd:documentation>Via point on line for publicity purposes (+TXC v2.4)</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="Description" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Description of line for publicity purposes (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="Track">		<xsd:annotation>			<xsd:documentation>A track describes a piece of the path of a  link that can be projected onto the geospatial model. The choice of how  links are divided into tracks is left to TransXChange implementors. For example, a major road junction might represent the end of one track and the beginning of the next.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="TrackStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="TrackStructure">		<xsd:annotation>			<xsd:documentation>Type for a Track: A track describes a piece of the path of a route link that can be projected onto the geospatial model. The choice of how route links are divided into tracks is left to TransXChange implementers. For example, a major road junction might represent the end of one track and the beginning of the next.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="RouteLinkRef" type="RouteLinkRefStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Optional reference to parent RouteLink. Normally not stated as given by context, but may be specified when using Track as a stand alone artefact. If given by context, this value is ignored. (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Mapping" type="LineStringStructure">						<xsd:annotation>							<xsd:documentation>Set of geospatial points which describe the track. Must include the beginning and end of the track.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="MapSystemReference" type="MapSystemReferenceStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>A mapping system reference (e.g. TOID, OSODR, NavTech, MDV) value which the requesting client has asked for.@MappingSystem</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Instructions" type="TrackInstructionsStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Information to describe a track textually in a step by step fashion.</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="TrackInstructionsStructure">		<xsd:annotation>			<xsd:documentation>Type for Track instructions</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Summary" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Optional textual description of track.  </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Feature" type="FeatureStructure" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Represents a physical obstacle in the route e.g. a crossing. The first feature point should be the start point of the track eg (the leg board point). Each feature contains directions to the next feature.@id.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="TrackIdType">		<xsd:annotation>			<xsd:documentation>Type for Track Identifier</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="TrackRefStructure">		<xsd:annotation>			<xsd:documentation>Type for Reference to a Track</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="LineIdType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="Feature" type="FeatureStructure">		<xsd:annotation>			<xsd:documentation>A Step in a set of instructions</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="FeatureStructure">		<xsd:annotation>			<xsd:documentation>Type for Represents a physical obstacle in the route e.g. a crossing. The first feature point should be the start point of the track eg (the leg board point). Each feature contains directions to the next feature.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="LocationRef" type="IdType">				<xsd:annotation>					<xsd:documentation>Refers to a spatial map location for the feature. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="FeatureType" type="FeatureTypeEnumeration">				<xsd:annotation>					<xsd:documentation>An enumeration of the different types of features.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="RelativeBearing" type="RelativeBearingEnumeration">				<xsd:annotation>					<xsd:documentation>Which way you would turn from this feature point.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="AbsoluteBearing" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The compass bearing which you should take directly from this feature point.</xsd:documentation>				</xsd:annotation>				<xsd:simpleType>					<xsd:restriction base="xsd:nonNegativeInteger">						<xsd:maxExclusive value="360"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="OnwardName" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The name of the road or path following this feature.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="RoadNumber" type="xsd:string" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The number of the road following this feature e.g. A1.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Distance" type="DistanceType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Provides the distance to the next feature point, or to the leg alight point for the last feature point.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Description" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Provides a description of the feature.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>		<xsd:attribute name="Id" type="IdType" use="optional">			<xsd:annotation>				<xsd:documentation>Refers to a geocode in the mapping information for the track segment.</xsd:documentation>			</xsd:annotation>		</xsd:attribute>	</xsd:complexType></xsd:schema>