<?xml version="1.0" encoding="UTF-8"?><!-- (c) Crown Copyright 2000-2005   --><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_service_journeyGrouping">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2010-01-31</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4 Modularised from TXC_common 							PTIC-999  XML Technical changes								Modularised from txc_common 	 						    XML technical Correction for constraints								Change type of StopPointRef to be a simple type  StopPointCodeType							PTIC-0012 Explicit Grouping Of journeys . Also BankHolidayOnly							PTIC-0014 Horizontal sorting of constraints 								 - Add ColumnSort StopPointRef -->				</Date>				<Description> Common service part element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_service_parts-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_operator-v2-3.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_operational-v2-3.xsd </Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_utility_types-v2-3.xsd </Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations,Rail transport, Infrastructure (railways), Train Bus stops and stations, Road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Service Part Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_ service_journeyGrouping   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="TXC_vehicleJourney-v2-3.xsd"/>	<!-- ====JourneyGrouping===================================================== -->	<xsd:simpleType name="JourneyGroupingIdType">		<xsd:annotation>			<xsd:documentation>Type for JourneyGrouping Identifier.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="JourneyGroupingsStructure">		<xsd:annotation>			<xsd:documentation>Type for an Abstract Journey Grouping.  (TXC v2.4).</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="OutboundJourneyGroupings" type="BuiltInJourneyGroupingsStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Built-in Groupings . These will always be used unless suppressed.  (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="InboundJourneyGroupings" type="BuiltInJourneyGroupingsStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Built-in Groupings. These will always be used unless suppressed.  (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="CustomJourneyGroupings" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Custom groupings. Any journeys assigned to this group will   (TXC v2.4)</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element ref="CustomJourneyGrouping" maxOccurs="unbounded">							<xsd:annotation>								<xsd:documentation>Custom Journey Grouping.  (TXC v2.4)</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:complexType name="AbstractJourneyGroupingStructure">		<xsd:annotation>			<xsd:documentation>Type for an Abstract Journey Grouping.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="GenerateTitle" type="xsd:boolean" default="true" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Whether a title should be generated from the OperatingProfile and direction, for example, 'Outbound, Monday To Friday'</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Description" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Text description of route to use for journey grouping.  Will be appended to any generated title</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Vias" type="ViasStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>A list of place names to use to create journey group description . Will be published as A-B-C-D etc. </xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="StopPointRef" type="StopPointRefStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Reference to a Stop Point whose departure times can be used for Horizontal sorting of columns within matrix.  (Not yet supported by Publisher in vTXC2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:sequence>						<xsd:element name="Notes" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Notes associated with Journey grouping.</xsd:documentation>							</xsd:annotation>							<xsd:complexType>								<xsd:sequence>									<xsd:element name="Note" type="NoteStructure" maxOccurs="unbounded">										<xsd:annotation>											<xsd:documentation>Note</xsd:documentation>										</xsd:annotation>									</xsd:element>								</xsd:sequence>							</xsd:complexType>						</xsd:element>					</xsd:sequence>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="BuiltInJourneyGroupingsStructure">		<xsd:annotation>			<xsd:documentation>Type for a set of Built in Journey Groupings.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="MondayToFridayJourneyGrouping" type="BuiltInJourneyGroupingStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Built in Grouping for Monday To Friday</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="SaturdayJourneyGrouping" type="BuiltInJourneyGroupingStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Built in Grouping for Monday To Friday</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="SundayJourneyGrouping" type="BuiltInJourneyGroupingStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Built in Grouping for Monday To Friday</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="HolidaysOnlyJourneyGrouping" type="BuiltInJourneyGroupingStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Built in Grouping for Holidays only.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="BuiltInJourneyGroupingStructure">		<xsd:annotation>			<xsd:documentation> Type for Built in Journey Grouping on a specific DayType/Direction. </xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="AbstractJourneyGroupingStructure">				<xsd:sequence>					<xsd:element name="Contents">						<xsd:annotation>							<xsd:documentation>Criteria for including</xsd:documentation>						</xsd:annotation>						<xsd:complexType>							<xsd:choice>								<xsd:element name="ByOperationalProfile" type="EmptyType">									<xsd:annotation>										<xsd:documentation>Include any vehicle journeys that have the same day type and direction and are not in a CustomJourneyGrouping.</xsd:documentation>									</xsd:annotation>								</xsd:element>								<xsd:element name="None" type="EmptyType">									<xsd:annotation>										<xsd:documentation>Suppress this grouping. </xsd:documentation>									</xsd:annotation>								</xsd:element>							</xsd:choice>						</xsd:complexType>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="CustomJourneyGrouping" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>Custom Journey Grouping</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="CustomJourneyGroupingStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="CustomJourneyGroupingStructure">		<xsd:annotation>			<xsd:documentation>Type for custom Journey Grouping</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="AbstractJourneyGroupingStructure">				<xsd:sequence>					<xsd:group ref="CustomJourneyGroupingGroup"/>				</xsd:sequence>				<xsd:attribute name="Sequence" type="xsd:integer">					<xsd:annotation>						<xsd:documentation>Sequence in which to print Group</xsd:documentation>					</xsd:annotation>				</xsd:attribute>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="CustomJourneyGroupingGroup">		<xsd:annotation>			<xsd:documentation> Elements defining a custom Journey Grouping </xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Direction" type="LinkDirectionEnumeration">				<xsd:annotation>					<xsd:documentation>Direction of journey. Journeys in Group must match this  </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="PrivateCode" type="PrivateCodeType" minOccurs="0"/>			<xsd:element name="VehicleJourneyRefs" minOccurs="0">				<xsd:annotation>					<xsd:documentation>vehicle Journeys in Group</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="VehicleJourneyRef" type="VehicleJourneyCodeType" maxOccurs="unbounded">							<xsd:annotation>								<xsd:documentation>Identifier of Vehicle Journey Which is to be included in Group</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= --></xsd:schema>