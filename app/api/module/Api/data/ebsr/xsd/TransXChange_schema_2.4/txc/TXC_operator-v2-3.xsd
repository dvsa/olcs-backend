<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_operator">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4b 			    EbsrUser moved to Registration    -->				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4a Modularised from TXC_Common-		change operator short name and licence name to normalised string 								  Make Operator & explicit versionedObject subtypes							      Add operator elements         PTIC-001 Operator  			  Add LicenceDateof Expiry			 Add LicenceHolderNames	     	Add OperatporReferenceName			  ParentOperatorRef (annotatedOperatorRef : Ref + names)			UltimateParentRef (annotatedOperatorRef)  			 Add PrimaryMode 			  Add EbsrUser flag			  Add ContactPersion			  Add ContaxFaxNumber			  Add CustomerServicesTelephoneNumber			  Add Web site address							PTIC-044 Additional Change Management Support - Deltas             Add regional opearetor codes , further comment & Ebsr Agent		  PTIC-031 Permission levels					Add data rights/Controbutor group, Policy STatus-->				</Date>				<Date>					<Created>2004-02-01</Created>				</Date>				<Description> Common Operator element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_operator-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_garage-v2-3.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/apd/AddressTypes-v1-3.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Operator Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_operator  HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="TXC_garage-v2-3.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_operator_support-v2-3.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_utility_rights-v2-3.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_modes-v2-2.xsd"/>	<xsd:include schemaLocation="../nptg/NPTG_administrative_support-v2-4.xsd"/>	<xsd:import namespace="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" schemaLocation="../apd/AddressTypes-v1-3.xsd">		<xsd:annotation>			<xsd:documentation source="http://www.govtalk.gov.uk/schemasstandards/agreedschema_schema.asp?schemaid=204">A set of Address and personal details schema, including BS7666 version 1.3</xsd:documentation>		</xsd:annotation>	</xsd:import>	<!-- ================== ====================== -====================================== -->	<xsd:element name="OperatorRef" type="OperatorRefStructure">		<xsd:annotation>			<xsd:documentation>Reference to an operator</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:simpleType name="OperatorCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for the operator.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:complexType name="OperatorRefStructure">		<xsd:annotation>			<xsd:documentation>A direct reference to an Operator that is defined elsewhere   The reference is by Operator code.</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="OperatorCodeType"/>		</xsd:simpleContent>	</xsd:complexType>	<xsd:complexType name="OperatorRefsStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more operator refs.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="OperatorRef" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="AnnotatedOperatorRefStructure">		<xsd:annotation>			<xsd:documentation>An annotated  reference to an Operator that is defined elsewhere   The reference is by Operator code.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:choice>				<xsd:element ref="OperatorRef"/>				<xsd:element name="NationalOperatorRef" type="NationalOperatorCodeType">					<xsd:annotation>						<xsd:documentation>Unique national identifier of operator.</xsd:documentation>					</xsd:annotation>				</xsd:element>			</xsd:choice>			<xsd:group ref="OperatorNamesGroup"/>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:simpleType name="OperatorPartialLicenceNumberType">		<xsd:annotation>			<xsd:documentation>Partial Licence Number of the operator.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:normalizedString"/>	</xsd:simpleType>	<xsd:simpleType name="OperatorLicenceNumberType">		<xsd:annotation>			<xsd:documentation>Full Licence Number of the operator,</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:normalizedString"/>	</xsd:simpleType>	<xsd:simpleType name="OperatorLicenceEnumeration">		<xsd:annotation>			<xsd:documentation>Licence type of the operator.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="standardNational"/>			<xsd:enumeration value="standardInternational"/>			<xsd:enumeration value="restricted"/>			<xsd:enumeration value="specialRestricted"/>			<xsd:enumeration value="communityBusPermit"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="LicenceStatusEnumeration">		<xsd:annotation>			<xsd:documentation>Licence type of the operator.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="Valid"/>			<xsd:enumeration value="Refused"/>			<xsd:enumeration value="Surrendered"/>			<xsd:enumeration value="ContinuationNotSought"/>			<xsd:enumeration value="Revoked"/>			<xsd:enumeration value="Withdrawn"/>			<xsd:enumeration value="ApplicationInProgress"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= -->	<xsd:simpleType name="OperatorShortNameType">		<xsd:annotation>			<xsd:documentation>Concise form of the operator name.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:normalizedString"/>	</xsd:simpleType>	<xsd:simpleType name="OperatorTradingNameType">		<xsd:annotation>			<xsd:documentation>Trading name of the operator (i.e. the operator's name known to passengers).</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:normalizedString"/>	</xsd:simpleType>	<xsd:complexType name="OperatorsStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more operators.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence maxOccurs="unbounded">					<xsd:element ref="Operator" minOccurs="0">						<xsd:annotation>							<xsd:documentation>A transport operator.@id,@CreationDateTime,@ModificationDateTime.@Modification,@RevisionNumber.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element ref="LicensedOperator" minOccurs="0"/>					<xsd:element ref="Contributor" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Type of orfanisation that is a Data right Owner</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ==Operator ========================================================== -->	<xsd:element name="Operator" type="OperatorStructure">		<xsd:annotation>			<xsd:documentation>A transport operator.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="OperatorStructure">		<xsd:annotation>			<xsd:documentation>A transport operator.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="NationalOperatorCode" type="NationalOperatorCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Unique national identifier of operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="OperatorCode" type="OperatorCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Identifier of operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="PrivateCode" type="PrivateCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Alternative code for oerator. TX v 2.4</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="OperatorNamesGroup"/>					<xsd:group ref="OperatorLicenceGroup">						<xsd:annotation>							<xsd:documentation>Elements for Operator primary Licence.</xsd:documentation>						</xsd:annotation>					</xsd:group>					<xsd:element name="OtherLicences" type="OperatorLicencesStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Other licences that the Operatoe holds (TXC 2.4).</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="OperatorParentGroup">						<xsd:annotation>							<xsd:documentation>Elements for Operator Parents.</xsd:documentation>						</xsd:annotation>					</xsd:group>					<xsd:group ref="OperatorInfoGroup">						<xsd:annotation>							<xsd:documentation>Elements for Operator further descri[ption (TXC v2.3)</xsd:documentation>						</xsd:annotation>					</xsd:group>					<xsd:group ref="OperatorContactGroup"/>					<xsd:element name="Garages" type="GaragesStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Bus garages that operator runs.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="ContributorGroup" minOccurs="0"/>					<xsd:element ref="Extensions" minOccurs="0"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="OperatorNamesGroup">		<xsd:annotation>			<xsd:documentation>Elements for Operator. Names</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="OperatorShortName" type="OperatorShortNameType">				<xsd:annotation>					<xsd:documentation>Short text name for operator.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="OperatorNameOnLicence" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Full name of the operator (i.e. as appears on licence). @lang.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="TradingName" type="OperatorTradingNameType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Operator Public Name. Name under which operator trades. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ReferenceName" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name used to distinguish operator from similalry named operators. For use by data providers and managers. Not normally for public use. (TXc v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OperatorLicenceGroup">		<xsd:annotation>			<xsd:documentation>Elements for Operator. Lcience.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="LicenceNumber" type="OperatorLicenceNumberType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Operator's licence number.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LicenceClassification" type="OperatorLicenceEnumeration" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Type of operator licence. Enumerated value.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LicenceExpiryDate" type="xsd:date" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Date of expiry oif licence. TXC v2.4</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LicenceHolderNames" type="LicenceHolderNamesStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Names of licence holders TXC v2.4</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LicenceStatus" type="LicenceStatusEnumeration" default="Valid" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Status of the Operator's licence, Default is valid. TXC v2.4</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OperatorContactGroup">		<xsd:annotation>			<xsd:documentation>Contact details for Operator.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="EnquiryTelephoneNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Public Enquiry telephone number.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactTelephoneNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact telephone number.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactFaxNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact Fax telephone number. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactPerson" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of contact persion (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="CustomerServiceTelephoneNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Customer Services Telephone number  (TXC v2.4).</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="OperatorAddresses" type="OperatorAddressesStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>One or more contact addresses for the operator.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="EmailAddress" type="EmailAddressType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The email address of the operator. It is up to the operator whether an individual's address, or a generic company e-mail address is used.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="WebSite" type="xsd:anyURI" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The web site address of the operator.  </xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OperatorParentGroup">		<xsd:annotation>			<xsd:documentation>Elements for Operator. Parents.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ParentOperatorRef" type="AnnotatedOperatorRefStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Immediate Parent of Operator. TXC v2.4</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="UltimateParentRef" type="AnnotatedOperatorRefStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Ultimate Parent of Operato. TXC v2.4</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OperatorInfoGroup">		<xsd:annotation>			<xsd:documentation>Elements for Operator. Parents.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="PrimaryMode" type="VehicleModesEnumeration" default="bus" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Primary mode that Operator runs.   (TXC 2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="EbsrUser" type="xsd:boolean" default="true" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Whether Operator provides timetables to EBSR. Default  is true. (TXC 2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="TravelineOwner" type="RegionShortCodeEnumeration" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Cuurremt Travelline owner of data.  (TXC 2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="RegionalOperatorRefs" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Mapping to legacy  regional codes.  (TXC 2.4) </xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="RegionalOperatorRef" type="RegionalOperatorCodeStructure" maxOccurs="unbounded">							<xsd:annotation>								<xsd:documentation>Mapping to Travelline code for a region</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="Note" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Further comment on operator.  (TXC 2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:complexType name="RegionalOperatorCodeStructure">		<xsd:annotation>			<xsd:documentation>Type for Operator Regional Codes</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedChildStructure">				<xsd:sequence>					<xsd:element name="TravellineRegion" type="RegionShortCodeEnumeration">						<xsd:annotation>							<xsd:documentation>Identifier of Traveline region.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="TravelineOperatorCode" type="OperatorCodeType">						<xsd:annotation>							<xsd:documentation>Operator code for specified region</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:complexType name="OperatorLicencesStructure">		<xsd:annotation>			<xsd:documentation>Type for Collection of Licences for the Operator  </xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="OperatorLicence" minOccurs="0" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>Operrator Licence</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="OperatorLicence" substitutionGroup="VersionedChild">		<xsd:annotation>			<xsd:documentation>Operator Licence</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="OperatorLicenceStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="OperatorLicenceStructure">		<xsd:annotation>			<xsd:documentation>Type for Operator Licence</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedChildStructure">				<xsd:group ref="OperatorLicenceGroup"/>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="LicenceHolderNamesStructure">		<xsd:annotation>			<xsd:documentation>Type for list of lcience holder names</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="HolderName" type="xsd:normalizedString" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Name of Licence Holder (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="LicensedOperator" type="LicensedOperatorStructure" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>A transport operator that includes licensing details that are mandatory for service registration.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="LicensedOperatorStructure">		<xsd:annotation>			<xsd:documentation>A transport operator that includes licensing details that are mandatory for service registration.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:restriction base="OperatorStructure">				<xsd:sequence>					<xsd:element name="NationalOperatorCode" type="NationalOperatorCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Unique national identifier of operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="OperatorCode" type="OperatorCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Identifier of operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="PrivateCode" type="PrivateCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Alternative code for oerator. TX v 2.4</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:sequence>						<xsd:element name="OperatorShortName" type="OperatorShortNameType">							<xsd:annotation>								<xsd:documentation>Short text name for operator.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="OperatorNameOnLicence" type="NaturalLanguageStringStructure">							<xsd:annotation>								<xsd:documentation>Full name of the operator (i.e. as appears on licence). @lang. Mandatory for a Licenced Soperator</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="TradingName" type="OperatorTradingNameType" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Name under which operator trades. </xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="ReferenceName" type="OperatorTradingNameType" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Name under which operator is referenced </xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>					<xsd:sequence>						<xsd:element name="LicenceNumber" type="OperatorLicenceNumberType">							<xsd:annotation>								<xsd:documentation>Operator's licence number.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="LicenceClassification" type="OperatorLicenceEnumeration">							<xsd:annotation>								<xsd:documentation>Type of operator licence. Enumerated value.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="LicenceExpiryDate" type="xsd:date" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Date of expiry oif licence. TXC v2.4</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="LicenceHolderNames" type="LicenceHolderNamesStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Names of licence holders TXC v2.4</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="LicenceStatus" type="LicenceStatusEnumeration" default="Valid" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Status of the Operator's licence, Default is valid. TXC v2.4</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>					<xsd:element name="OtherLicences" type="OperatorLicencesStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Other Licences for opertaor</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="OperatorParentGroup"/>					<xsd:group ref="OperatorInfoGroup"/>					<xsd:group ref="LicensedOperatorContactGroup"/>					<xsd:element name="Garages" type="GaragesStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Bus garages that operator runs.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="ContributorGroup" minOccurs="0"/>					<xsd:element ref="Extensions" minOccurs="0"/>				</xsd:sequence>			</xsd:restriction>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="LicensedOperatorContactGroup">		<xsd:annotation>			<xsd:documentation>Contact details for a Licensed Operator. Has Mandatory elements.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="EnquiryTelephoneNumber" type="TelephoneContactStructure">				<xsd:annotation>					<xsd:documentation>Enquiry telephone number of operator.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactTelephoneNumber" type="TelephoneContactStructure">				<xsd:annotation>					<xsd:documentation>Contact telephone number of operator.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactFaxNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact Fax telephone number.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactPerson" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of contact persion (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="CustomerServiceTelephoneNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Customer Services Telephone number  (TXC v2.4).</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="OperatorAddresses" type="OperatorAddressesStructure">				<xsd:annotation>					<xsd:documentation>One or more contact addresses for the operator.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="EmailAddress" type="EmailAddressType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The email address of the operator. It is up to the operator whether an individual's address, or a generic company e-mail address is used.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="WebSite" type="xsd:anyURI" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The web site address of the operator.  </xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:complexType name="OperatorRoleStructure">		<xsd:annotation>			<xsd:documentation>An operator and a role.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence maxOccurs="unbounded">					<xsd:element name="OperatorRef" type="OperatorCodeType">						<xsd:annotation>							<xsd:documentation>Reference to an operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Role" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Role that associated operator performs</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="OperatorAddressesStructure">		<xsd:annotation>			<xsd:documentation>Collection of addresses for the operator (as a minimum this must include the correspondence address).</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element name="CorrespondenceAddress" type="apd:UKPostalAddressStructure">						<xsd:annotation>							<xsd:documentation>Address to be used for correspondence (NB. this is NOT the enquiry address for public usage).</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="MiscellaneousAddress" type="apd:UKPostalAddressStructure" minOccurs="0" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>Other Postal Addresses (NB. this is NOT the correspondence address).</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType></xsd:schema>