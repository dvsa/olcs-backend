<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_serviceCalendar">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Added for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 24 Nicholas Knowles, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2010-03-01</Created>				</Date>				<Date>					<Modified>2010-03-151</Modified>					<!-- 2.4 							PTIC-035 General day support 							PTIC-027 Multiple References per journey									add   day type			   PTIC-071 National Terms Database  Holiday types: 							  -->				</Date>				<Description> Common Calendar  element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_validity-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/napt/NaPT_administrativeSupport-v2-42.xsd"</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_validity-v2-3.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.4 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Calendar Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_calendar   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ============================================================ -->	<xsd:include schemaLocation="../nptg/NPTG_administrative_support-v2-4.xsd"/>	<xsd:include schemaLocation="TXC_validity-v2-3.xsd"/>	<xsd:include schemaLocation="TXC_vehicleJourney_support-v2-3.xsd"/>	<!-- === Operational Profiles ================================================== -->	<!-- ==Day Type========================================================== -->	<xsd:simpleType name="CalendarIdType">		<xsd:annotation>			<xsd:documentation>Type for Calendar</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:complexType name="ServiceCalendarsStructure">		<xsd:annotation>			<xsd:documentation>Type for  list of Service Calendar</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="ServiceCalendar" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="ServiceCalendar" type="ServiceCalendarStructure">		<xsd:annotation>			<xsd:documentation>Operating Calendar  (TXC v2.4)@id.@Modification Attributes</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="ServiceCalendarStructure">		<xsd:annotation>			<xsd:documentation>Type for Calendar</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="CalendarPeriod" type="ClosedDateRangeStructure">						<xsd:annotation>							<xsd:documentation>Period of calends (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Name" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Name of Calendar  (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="OperatingDays" type="OperatingDaysStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Days in calendar  (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="DayTypes" type="DayTypesStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Reusable Day Types  (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="OperatingDaysStructure">		<xsd:annotation>			<xsd:documentation>Type for list of operating days</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element name="OperatingDay" type="OperatingDayStructure" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>Operating day that calendar date is assigned to</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ====Operating Day======================================================= -->	<xsd:simpleType name="OperatingDayIdType">		<xsd:annotation>			<xsd:documentation>Identifying code for a particular service.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="IdType"/>	</xsd:simpleType>	<xsd:element name="OperatingDay" type="OperatingDayStructure">		<xsd:annotation>			<xsd:documentation>Operating Day. (Txc v2.4)@id.@Modification Attributes</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="OperatingDayStructure">		<xsd:annotation>			<xsd:documentation>Type for Operating Day</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="Date" type="xsd:date">						<xsd:annotation>							<xsd:documentation>Date of operating day</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="StartTime" type="xsd:time" default="00:00:00" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Start time. Default is 00:00</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Duration" type="xsd:duration" default="PT24H" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Duration of day. Default is 24 hours</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element ref="ServiceDayAssignment" minOccurs="0"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ===Day Assignment================================================= -->	<xsd:complexType name="DayAssignmentStructure" abstract="true">		<xsd:annotation>			<xsd:documentation>Type for a day assignment</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedChildStructure">				<xsd:sequence>					<xsd:element name="Description" type="NaturalLanguageStringStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Further Description of assignment.  (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Holiday" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Identification of the day as a holiday. Optional  </xsd:documentation>						</xsd:annotation>						<xsd:complexType>							<xsd:choice>								<xsd:element name="HolidayName">									<xsd:annotation>										<xsd:documentation>TransXChange Name of Statutory  Bank Holiday</xsd:documentation>									</xsd:annotation>									<xsd:complexType>										<xsd:sequence>											<xsd:group ref="BankHolidayChoiceGroup" minOccurs="0"/>										</xsd:sequence>									</xsd:complexType>								</xsd:element>								<xsd:element name="SpecialHoliday" type="PublicHolidayStructure">									<xsd:annotation>										<xsd:documentation>name of other Holiday</xsd:documentation>									</xsd:annotation>								</xsd:element>							</xsd:choice>						</xsd:complexType>					</xsd:element>					<xsd:element name="Country" type="CountryEnumeration" default="UK" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Scope of holiday. Default is UK.  (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="ServiceDayAssignment" type="ServiceDayAssignmentStructure">		<xsd:annotation>			<xsd:documentation>Assignment of a Day type to a Calendar date</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="ServiceDayAssignmentStructure">		<xsd:annotation>			<xsd:documentation>Type for a day assignment</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DayAssignmentStructure">				<xsd:sequence>					<xsd:choice>						<xsd:element name="DayTypeRef" type="DayTypeRefStructure">							<xsd:annotation>								<xsd:documentation>Day Type to be applied on operating day. (TXC v2.4). Journeys that match all the properties of this day type will be run, within the time window specified for the operating day. </xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="NoService" type="EmptyType">							<xsd:annotation>								<xsd:documentation>There is no service on the specified day. (TXC v2.4)</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="UnspecifiedService" type="EmptyType">							<xsd:annotation>								<xsd:documentation>Service for this day has not yet been specified. (TXC v2.4)</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="VehicleJourneyRefs">							<xsd:annotation>								<xsd:documentation>The following explicit list of vehicle journeys will be run. (TXC v2.4)</xsd:documentation>							</xsd:annotation>							<xsd:complexType>								<xsd:sequence>									<xsd:element name="VehicleJourneyRef" type="VehicleJourneyRefStructure" maxOccurs="unbounded">										<xsd:annotation>											<xsd:documentation>Identifier of a vehicle Journey</xsd:documentation>										</xsd:annotation>									</xsd:element>								</xsd:sequence>							</xsd:complexType>						</xsd:element>					</xsd:choice>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType></xsd:schema>