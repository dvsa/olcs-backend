<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_connectingVehicleJourney">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Added for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.4 Nicholas Knowles Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2010-032-151</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4  Modularised from txc_common  							PTIC-039 Journey Interchanges.  								    Add connecting VehicleJourney                                    Add sequence number for connecting vehicle Journey					 -->				</Date>				<Description> Connecting  Vehicle Journey element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_connectingVehicleJourney-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_registration-v2-3.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_vehicleJourney-v2-3.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.4 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Connecting Vehicle Journey Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_connectingVehicleJourney   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<xsd:include schemaLocation="TXC_vehicleJourney-v2-3.xsd"/>	<xsd:include schemaLocation="TXC_registration-v2-3.xsd"/>	<!-- ======================================================================= -->	<xsd:complexType name="VehicleJourneysStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more vehicle journeys.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence maxOccurs="unbounded">					<xsd:element ref="VehicleJourney" minOccurs="0"/>					<xsd:element ref="FlexibleVehicleJourney" minOccurs="0"/>					<xsd:element ref="ConnectingVehicleJourney" minOccurs="0"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ==Connecting Vehicle Journey============================================== -->	<xsd:element name="ConnectingVehicleJourney" substitutionGroup="Journey">		<xsd:annotation>			<xsd:documentation>A specific scheduled journey by an individual vehicle making up a standard part of a service.@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber@Status,@SequenceNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="ConnectingVehicleJourneyStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="ConnectingVehicleJourneyStructure">		<xsd:annotation>			<xsd:documentation>Externally defined vehicle Journey.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="AbstractServicePatternStructure">				<xsd:sequence>					<xsd:group ref="ConnectingVehicleJourneyGroup"/>				</xsd:sequence>				<xsd:attribute name="SequenceNumber" type="xsd:positiveInteger" use="optional">					<xsd:annotation>						<xsd:documentation>Presentation Ordering for Journey</xsd:documentation>					</xsd:annotation>				</xsd:attribute>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="ConnectingTimeGroup">		<xsd:annotation>			<xsd:documentation>Properties specific to a standard vehicle journey.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ArrivalTime" type="xsd:time" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Time of departure from origin stop of the Vehicle Journey.   </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ArrivalDayShift" type="xsd:integer" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Shift in days forward or backwards if time is not on same day as day type of operating profile .  For example a 00:10 Saturday service that was part of Friday service would have Day Shift + 1,  A 23:50 Sunday service that was part of Monday to Friday service would have Day Shift  -1.  The Oprtaing profiel can be used to specify further details </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="DepartureTime" type="xsd:time" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Time of departure from origin stop of the Vehicle Journey.   </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="DepartureDayShift" type="xsd:integer" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Shift in days forward or backwards if time is not on same day as day type of operating profile .  For example a 00:10 Saturday service that was part of Friday service would have Day Shift + 1,  A 23:50 Sunday service that was part of Monday to Friday service would have DayShift  -1. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Frequency" type="FrequentServiceStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Frequency details for a frequent service, that is one that runs as a shuttle rather than to a set timetable.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="ConnectingVehicleJourneyGroup">		<xsd:annotation>			<xsd:documentation>Properties specific to a standard vehicle journey.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="VehicleJourneyCode" type="VehicleJourneyCodeType">				<xsd:annotation>					<xsd:documentation>Identifies the Connecting vehicle Journey</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ServiceRef" type="ServiceNumberType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The Service that the VehicleJourney contributes to. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="RegistrationRef" minOccurs="0"/>			<xsd:element name="AnnotatedOperatorRef" type="AnnotatedOperatorRefStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Reference to an Operator</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="AnnotatedLineRef" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Reference to a line</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="LineRef" type="LineIdType" minOccurs="0">							<xsd:annotation>								<xsd:documentation>The Line for which the VehicleJourney operates.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="LineName" type="NaturalLanguageStringStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Public identifier of the service. E.g. "N73".</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:group ref="ConnectingTimeGroup"/>			<xsd:element name="VehicleJourneyInterchange" minOccurs="0" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Information about an interchange where the vehicle journey connects with  another vehicle journey. </xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:complexContent>						<xsd:extension base="VehicleJourneyInterchangeStructure"/>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>			<xsd:element name="Note" type="NoteStructure" minOccurs="0" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Any additional notes on the VehicleJourney.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= --></xsd:schema>