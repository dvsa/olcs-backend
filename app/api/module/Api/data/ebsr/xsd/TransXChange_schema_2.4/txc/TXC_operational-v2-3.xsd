<?xml version="1.0" encoding="UTF-8"?><!-- (c) Crown Copyright 2000-2005   --><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_operational">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2004-02-01</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4 Modularised from TXC Common							PTIC-027 add explicit BlockNumberType							PTIC-029 Add Basic vehicle Equipment types for accessibility explicit BlockNumberType-->				</Date>				<Description> Common operational element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_operational-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_types-v2-3.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/txc/TXC_journey-v2-3.xsd</Requires>					<Requires>http://www.w3.org/2001/xml.xsd</Requires>					<Requires>apd/AddressTypes-v1-3.xsd</Requires>				</Relation>				<Relation>					<HasPart>http://www.transxchange.org.uk/schema/2.4/txc/TXC_types.xsd</HasPart>					<isPartOf>http://www.transxchange.org.uk/schema/2.4/txc/TXC_general.xsd</isPartOf>					<isPartOf>http://www.transxchange.org.uk/schema/2.4/txc/TXC_registration.xsd</isPartOf>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Operational Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_ operational   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="TXC_journey_support-v2-3.xsd"/>	<xsd:include schemaLocation="TXC_validity-v2-3.xsd"/>	<xsd:include schemaLocation="TXC_vehicleEquipment-v2-3.xsd"/>	<!-- ======================================================================= -->	<!--Operational data common types -->	<xsd:simpleType name="BlockNumberType">		<xsd:annotation>			<xsd:documentation>Identifying code for a Block</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:simpleType name="DutyCrewCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for a bus crew.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:simpleType name="TicketMachineJourneyCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for used by the ticket machine system to refer to a journey.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:simpleType name="TicketMachineServiceCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for used by the ticket machine system to refer to a service.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN"/>	</xsd:simpleType>	<xsd:simpleType name="FareStageNumberType">		<xsd:annotation>			<xsd:documentation>A fare stage number.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:nonNegativeInteger">			<xsd:maxInclusive value="999"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= -->	<xsd:element name="Operational" type="OperationalStructure">		<xsd:annotation>			<xsd:documentation>Operational data associated with element</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="OperationalStructure">		<xsd:annotation>			<xsd:documentation>Type for Data elements of use for an operational management</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:group ref="OperationalGroup">				<xsd:annotation>					<xsd:documentation>Default set of Operational data elements associated with element</xsd:documentation>				</xsd:annotation>			</xsd:group>			<xsd:element name="OperationalVariants" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Override values for operational data  to use on a particular operating day. TXC v2.4</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="OperationalVariant" type="OperationalVariantStructure" maxOccurs="unbounded">							<xsd:annotation>								<xsd:documentation>Override values for operational data  to use when a particular service day type is true. TXC v2.4</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:group name="OperationalGroup">		<xsd:annotation>			<xsd:documentation>Operational data elements associated with element</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Block" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Data elements used to associate journey with a block (running board).</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="Description" type="NaturalLanguageStringStructure">							<xsd:annotation>								<xsd:documentation>Description of the block (running board) of the journey. @lang</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="BlockNumber" type="BlockNumberType" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Reference number for the Block or Running Board.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="Note" type="NaturalLanguageStringStructure" minOccurs="0">							<xsd:annotation>								<xsd:documentation>Additional explanation of use of block for operations staff.</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="VehicleType" type="VehicleTypeStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The type of vehicle running the service.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="TicketMachine" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Data elements used to associate the journey with ticket machine settings.</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element name="TicketMachineServiceCode" type="TicketMachineJourneyCodeType" minOccurs="0">							<xsd:annotation>								<xsd:documentation>The identifier used by the ticket machine system to refer to the service. Overrides any service level value</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="JourneyCode" type="TicketMachineJourneyCodeType" minOccurs="0">							<xsd:annotation>								<xsd:documentation>The identifier used by the ticket machine system to refer to the journey.</xsd:documentation>							</xsd:annotation>						</xsd:element>						<xsd:element name="Direction" type="ServiceDirectionEnumeration" minOccurs="0">							<xsd:annotation>								<xsd:documentation>The direction used by the ticket machine system.</xsd:documentation>							</xsd:annotation>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:complexType name="OperationalVariantStructure">		<xsd:annotation>			<xsd:documentation>Type for Operational data associated with element</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:group ref="ValidityGroup"/>			<xsd:group ref="OperationalGroup"/>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= --></xsd:schema>