<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_utility_times">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2004-02-01</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4  Modularised from txc_common							PTIC-044 Additional Change Management Support - Deltas							PTIC075  Add public /private flag to notes							  -->				</Date>				<Description> Common Utility  Type element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_utility_types-v2-3</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.4/napt/NaPT_utility_types-v2-1.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.4/napt/NaPT_location-v2-1.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Utility  Type  Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_utility_types   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<xsd:include schemaLocation="../napt/NaPT_utility_types-v2-1.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_utility_rights-v2-3.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_location-v2-1.xsd"/>	<!-- Group Definitions -->	<xsd:attributeGroup name="DocumentMetadataGroup">		<xsd:annotation>			<xsd:documentation>Grouping for document metadata.</xsd:documentation>		</xsd:annotation>		<xsd:attribute name="SchemaVersion" type="xsd:NMTOKEN" use="required" fixed="2.4">			<xsd:annotation>				<xsd:documentation>The version of the schema that the TransXChange instance document was written against. This attribute conforms with e-GIF guidance.</xsd:documentation>			</xsd:annotation>		</xsd:attribute>		<xsd:attribute name="MappingSystem" type="MappingSystemEnumeration" use="optional">			<xsd:annotation>				<xsd:documentation>Identifies the default map reference system of Map element IDs (e.g. OSODRs or TOIDs) of any mapping layer references used.</xsd:documentation>			</xsd:annotation>		</xsd:attribute>		<xsd:attribute name="DataRightRef" type="DataRightIdType" use="optional">			<xsd:annotation>				<xsd:documentation>Data right that governs use of contents of document.</xsd:documentation>			</xsd:annotation>		</xsd:attribute>	</xsd:attributeGroup>	<!--======= Supporting Document =============-->	<xsd:complexType name="SupportingDocumentsStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more supporting documents.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element ref="SupportingDocument" maxOccurs="unbounded"/>		</xsd:sequence>		<xsd:attribute name="delta" type="xsd:boolean" default="false">			<xsd:annotation>				<xsd:documentation>Whether  only changes are included. Default is false</xsd:documentation>			</xsd:annotation>		</xsd:attribute>	</xsd:complexType>	<xsd:element name="SupportingDocument" type="SupportingDocumentStructure">		<xsd:annotation>			<xsd:documentation>A document supporting the associated element. Associated element may be (a)  a specific individual registration, or (b) the whole document including all components. </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="SupportingDocumentStructure">		<xsd:annotation>			<xsd:documentation>A document submitted as part of a Service Registration. Note that where such documentation is not available but is expected, then a supporting document should be supplied which lists the outstanding supporting documentation to be supplied</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="DocumentUri" type="xsd:anyURI">				<xsd:annotation>					<xsd:documentation>URI to locate a supporting document. For example can be used to provide relative  to supporting documents supplied with a registration submission. If the supporting files are included in the same directory as the Xml document a relative URL, i.e. just the filename, should be used.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="ImageDocumentType">		<xsd:annotation>			<xsd:documentation>Types of image </xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string"/>	</xsd:simpleType>	<xsd:simpleType name="TransXChangeDocumentRightsEnumeration">		<xsd:annotation>			<xsd:documentation>Enumeration of summary data rights.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Stops">				<xsd:annotation>					<xsd:documentation>Data has not been verified.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="StopsLocations">				<xsd:annotation>					<xsd:documentation>Data has   been verified by a community organisation.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="RoutesAndLines">				<xsd:annotation>					<xsd:documentation>Data has   been verified by a professional organisation.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="RouteGeometry"/>			<xsd:enumeration value="JourneyPatterns"/>			<xsd:enumeration value="VehicleJourneys"/>			<xsd:enumeration value="OperationalData"/>			<xsd:enumeration value="All"/>			<xsd:enumeration value="None"/>		</xsd:restriction>	</xsd:simpleType>	<!--===== Note  ===============-->	<xsd:complexType name="NotesStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more notes</xsd:documentation>		</xsd:annotation>		<xsd:choice>			<xsd:element ref="Note" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Footnote associated with another element</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="None" type="EmptyType">				<xsd:annotation>					<xsd:documentation>Suppress any inherited notes.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:choice>		<xsd:attribute name="delta" type="xsd:boolean" default="false">			<xsd:annotation>				<xsd:documentation>Whether  only changes are included. Default is false</xsd:documentation>			</xsd:annotation>		</xsd:attribute>	</xsd:complexType>	<xsd:simpleType name="NoteCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for a note.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:maxLength value="5"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:element name="Note" type="NoteStructure">		<xsd:annotation>			<xsd:documentation>Footnote element</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="NoteStructure">		<xsd:annotation>			<xsd:documentation>Note normally rendered with a timetable, and associated with either a service or an individual VehicleJourney</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="NoteCode" type="NoteCodeType">				<xsd:annotation>					<xsd:documentation>Reference code for Note.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="NoteText" type="NaturalLanguageStringStructure">				<xsd:annotation>					<xsd:documentation>Descriptive text of Note. @lang.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Private" type="xsd:boolean" default="false" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Whether note is only for internal use. Used by TXC publisher to include/exclude the note.  Default is false. (TXC 2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<!-- ======================================================================= --></xsd:schema>