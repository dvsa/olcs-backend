<?xml version="1.0" encoding="UTF-8"?><!-- (c) Crown Copyright 2000-2005   --><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.3" id="TXC_service">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.0 Kizoom Limited, by Tom White, Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.0 Tom White, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4b Modularised from TXC Common          PTIC-029 Add Wheelchair accessible flag to service -->				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4a Modularised from TXC Common 						PTIC-003 Service element combinations relaxed to allow  Rural and School works 						 PTIC-033 Add recommended end date to operating period						PTIC-038 Add Commercial basis flag for service.						PTIC-044 Additional Change Management Support - Deltas					                        PTIC-013 Line Direction									 	 Add Return Vias to Service                                         Refactor to add Origin,  Destination, Vias to Flexible service as well. allowed-->				</Date>				<Date>					<Created>2004-02-01</Created>				</Date>				<Description> Common Service Element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.4/txc/}TXC_service-v2-3.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<HasPart>http://www.transxchange.org.uk/schema/2.4/txc/TXC_journeyPattern-v2-3.xsd</HasPart>					<HasPart>http://www.transxchange.org.uk/schema/2.4/txc/TXC_journeyPatternInterchange-v2-3.xsd</HasPart>					<HasPart>http://www.transxchange.org.uk/schema/2.4/txc/TXC_flexibleJourneyPattern-v2-3.xsd</HasPart>					<HasPart>http://www.transxchange.org.uk/schema/2.4/txc/TXC_service_parts-v2-3.xsd</HasPart>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2010</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Service Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_ service   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="TXC_flexibleJourneyPattern-v2-3.xsd"/>	<xsd:include schemaLocation="TXC_service_journeyGrouping-v2-3.xsd"/>	<xsd:include schemaLocation="TXC_service_parts-v2-3.xsd"/>	<!-- ====Service========================================================= -->	<xsd:complexType name="ServicesStructure">		<xsd:annotation>			<xsd:documentation>Type for a collection of services.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element ref="Service" maxOccurs="unbounded"/>		</xsd:sequence>		<xsd:attribute name="delta" type="xsd:boolean" default="false">			<xsd:annotation>				<xsd:documentation>Whether  only changes are included. Default is false</xsd:documentation>			</xsd:annotation>		</xsd:attribute>	</xsd:complexType>	<xsd:element name="Service" substitutionGroup="VersionedObject">		<xsd:annotation>			<xsd:documentation>A bus service.@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="ServiceStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="ServiceStructure">		<xsd:annotation>			<xsd:documentation>Type for Classification and description of the bus service. This does not identify the individual bus journeys (defined as part of VehicleJourney)</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectStructure">				<xsd:sequence>					<xsd:element name="ServiceCode" type="ServiceCodeType">						<xsd:annotation>							<xsd:documentation>Code that uniquely identifies the Service.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="PrivateCode" type="PrivateCodeType" minOccurs="0">						<xsd:annotation>							<xsd:documentation>A private code that uniquely identifies the service. May be used for interoperating with other information systems.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Lines">						<xsd:annotation>							<xsd:documentation>The lines that make up the service.</xsd:documentation>						</xsd:annotation>						<xsd:complexType>							<xsd:sequence>								<xsd:element ref="Line" maxOccurs="unbounded"/>							</xsd:sequence>						</xsd:complexType>					</xsd:element>					<xsd:element name="OperatingPeriod" type="ServicePeriodStructure">						<xsd:annotation>							<xsd:documentation>Inclusive period within which Service runs.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="OperatingProfile" type="OperatingProfileStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Pattern of normal and special days defining the operational profile of the service. Default is Monday to Friday, every day of the year. </xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="JourneyGroupings" type="JourneyGroupingsStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>?????? Any subgroupings of journeys used within service Provides a means of collating and grouping journeys fro presentation. (TXC v2.4)</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="ServiceClassification" type="ServiceClassificationStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Nature of Service, categorisation for statutory purpose.  </xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="ServiceOperationalGroup"/>					<xsd:element name="RegisteredOperatorRef" type="OperatorRefStructure">						<xsd:annotation>							<xsd:documentation>Registered Operator of Service - i.e. with responsibility for the service. For registrations this must refer to a licensed operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="AssociatedOperators" type="OperatorRoleStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Operator associated with the Service who is NOT the Registered Operator.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="ServiceInfoGroup"/>					<xsd:group ref="ServiceDescriptionGroup"/>					<xsd:group ref="ServiceComponentGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="ServiceOperationalGroup">		<xsd:annotation>			<xsd:documentation>Operational elementsfor service. Specifies defaults that may bve overiided at some other levels</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="TicketMachineServiceCode" type="TicketMachineServiceCodeType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The identifier used by the ticket machine system to refer to the service.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="VehicleType" type="VehicleTypeStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Vehicle type normally used on service</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="ServiceComponentGroup">		<xsd:annotation>			<xsd:documentation>Journey properties of a service.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element ref="StandardService" minOccurs="0"/>			<xsd:element ref="FlexibleService" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Flexible Service Part </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Direction" type="ServiceDirectionEnumeration" default="outbound" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Direction of service. Provides default that can be overridden by JourneyPatterns. Defaults to outbound..  Enumerated value.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="JourneyPatternInterchange" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:group>	<!-- ====Service Part========================================================= -->	<xsd:complexType name="ServicePartStructure">		<xsd:annotation>			<xsd:documentation>Type for a classification and description of the bus service. This does not identify the individual bus journeys (defined as part of VehicleJourney)</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Origin" type="NaturalLanguageStringStructure">				<xsd:annotation>					<xsd:documentation>Public name of place where the Service Starts. @lang.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Destination" type="NaturalLanguageStringStructure">				<xsd:annotation>					<xsd:documentation>Public name of place where the Service Ends. @lang.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Vias" type="ViasStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Public names of main points on route. In order of the service direction</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ReturnVias" type="ViasStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Public names of main points on opposite direction. If absent assumed to be the Vias in inverse order.  (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="UseAllStopPoints" type="xsd:boolean" default="true" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Whether Service is to be registered will use all stops along its Route.  Default is true.    </xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:group name="ServicePartGroup">		<xsd:annotation>			<xsd:documentation>Common Journey properties of a service.part</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="StandardService" type="StandardServiceStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>A bus service other than a flexibly routed one. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="FlexibleService" type="FlexibleServiceStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>A service with variable pick-up points and no fixed operational schedule that is registered as a flexible service. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Direction" type="ServiceDirectionEnumeration" default="outbound" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Direction of service. Provides default that can be overridden by JourneyPatterns. Defaults to outbound..  Enumerated value.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="JourneyPatternInterchange" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:group>	<xsd:element name="StandardService">		<xsd:annotation>			<xsd:documentation>Standard Service Part </xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="StandardServiceStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="StandardServiceStructure">		<xsd:annotation>			<xsd:documentation>Type for a Standard Service Part </xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="ServicePartStructure">				<xsd:sequence>					<xsd:group ref="StandardServiceGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="StandardServiceGroup">		<xsd:annotation>			<xsd:documentation>Properties of a Standard Service Part </xsd:documentation>		</xsd:annotation>		<xsd:choice>			<xsd:element name="JourneyPattern" type="JourneyPatternStructure" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Pattern of journeys making up the service.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Cancellation" type="EmptyType">				<xsd:annotation>					<xsd:documentation>If registration is a cancellation, then a journey pattern can be omitted. Otherwise a Journey pattern must be specified. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:choice>	</xsd:group>	<xsd:element name="FlexibleService">		<xsd:annotation>			<xsd:documentation>Standard Service Part </xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="FlexibleServiceStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="FlexibleServiceStructure">		<xsd:annotation>			<xsd:documentation>Type for a Flexible Service part</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="ServicePartStructure">				<xsd:sequence>					<xsd:group ref="FlexibleServiceGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="FlexibleServiceGroup">		<xsd:annotation>			<xsd:documentation>Properties of a Standard Service Part </xsd:documentation>		</xsd:annotation>		<xsd:choice>			<xsd:element name="FlexibleJourneyPattern" type="FlexibleJourneyPatternStructure" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Pattern of journeys making up the service.@id,@CreationDateTime,@ModificationDateTime,@Modification,@RevisionNumber.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Cancellation" type="EmptyType">				<xsd:annotation>					<xsd:documentation>If registration is a cancellation, then a journey pattern can be omitted. Otherwise a Journey pattern must be specified. (TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:choice>	</xsd:group></xsd:schema>