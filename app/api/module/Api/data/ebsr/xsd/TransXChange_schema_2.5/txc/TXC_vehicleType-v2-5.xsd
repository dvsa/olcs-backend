<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" xmlns:ns1="http://www.govtalk.gov.uk/core" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.5" id="TXC_vehicleEquipment_support">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Added for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.4  Nicholas Knowles, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Modified>2013-013-13</Modified>					<!-- 2.5         PTIC-087 Add AssistanceService  -->				</Date>				<Date>					<Modified>2010-08-09</Modified>					<!-- 2.4  PTIC-29 Add sumary flag to vehicle equipment-->				</Date>				<Date>					<Created>2010-02-20</Created>					<!-- 2.4  new for PTIC-29 Low floor flags							  -->				</Date>				<Description> Common  Vehicle   Equipment  element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.5/txc/}TXC_vehicleEquipment-v2-5.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.5/napt/NaPT_utility_types-v2-5.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.5/napt/NaPT_stopAccessibility-v2-5.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.5/apd/CommonSimpleTypes-v1-3.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2013</Copyright>				</Rights>				<Source>New in 24 schema based on NeTEx</Source>				<Status>Version 2.5</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Infrastructure (railways), Train services, Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common  Vehicle Equipment type Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_vehicleEquipment   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<xsd:import namespace="http://www.govtalk.gov.uk/core" schemaLocation="../apd/CommonSimpleTypes-v1-3.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_utility_types-v2-5.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_stopAccessibility-v2-5.xsd"/>	<!-- ======================================================================= -->	<xsd:simpleType name="VehicleTypeCodeType">		<xsd:annotation>			<xsd:documentation>Identifying code for a vehicle type.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:maxLength value="8"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:complexType name="VehicleTypeStructure">		<xsd:annotation>			<xsd:documentation>Classification of type of vehicle for journey.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="VehicleTypeCode" type="VehicleTypeCodeType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Vehicle Type  code.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Description" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Vehicle Type short description. @lang.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="VehicleEquipment" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Equipment available on vehicle type (+TXC v2.4)</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:sequence>						<xsd:element ref="AccessVehicleEquipment" minOccurs="0"/>						<xsd:element ref="PassengerInfoEquipment" minOccurs="0"/>						<xsd:element ref="WheelchairVehicleEquipment" minOccurs="0"/>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="WheelchairAccessible" type="xsd:boolean" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Summary charaterisation as to  whether vehicle is considered wheelchair accessible.  (+TXC v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="Extensions" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<!-- ==== QUIPMENT ========================= -->	<xsd:element name="PassengerEquipment" type="PassengerEquipmentStructure" abstract="true">		<xsd:annotation>			<xsd:documentation>Passsenger equipment</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="PassengerEquipmentStructure">		<xsd:annotation>			<xsd:documentation>Type for Passenger Equipment for a Vehicle </xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="EquipmentStructure"/>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="PassengerInfoEquipment" substitutionGroup="PassengerEquipment">		<xsd:annotation>			<xsd:documentation>Passenger info Equipment for a Vehicle </xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="PassengerInfoEquipmentStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="PassengerInfoEquipmentStructure">		<xsd:annotation>			<xsd:documentation>Type for Passenger Equipment for a Vehicle </xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="PassengerEquipmentStructure">				<xsd:sequence>					<xsd:element ref="PassengerInfo" minOccurs="0" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>Passenger Info Equipment for a Vehicle </xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="AccessibilityInfo" type="AccessibilityInfoEnumeration" minOccurs="0" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>Accessibility Info for a Vehicle</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="PassengerInfo" type="PassengerInfoFaciltyEnumeration">		<xsd:annotation>			<xsd:documentation>Passenger Equipment for a Vehicle </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:simpleType name="PassengerInfoFaciltyEnumeration">		<xsd:annotation>			<xsd:documentation>Passenger info facility</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="nextStopIndicator"/>			<xsd:enumeration value="stopAnnouncements"/>			<xsd:enumeration value="passengerInfoFacility"/>			<xsd:enumeration value="other"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="AccessibilityInfoEnumeration">		<xsd:annotation>			<xsd:documentation>Accessibility Info</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="audioInformation"/>			<xsd:enumeration value="audioForHearingImpaired"/>			<xsd:enumeration value="visualDisplays"/>			<xsd:enumeration value="displaysForVisuallyImpaired"/>			<xsd:enumeration value="tactilePlatformEdges"/>			<xsd:enumeration value="tactileGuidingStrips"/>			<xsd:enumeration value="largePrintTimetables"/>			<xsd:enumeration value="other"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= --></xsd:schema>