<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.5" id="NaPT_TariffZone">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Input  for version 2.5 Kizoom Limited, by Jonathan Shewel Cooper. ATOS origin</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.5 TNicholas Knowles, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2013-03-21</Created>				</Date>				<Date>					<Modified>2013-04-05</Modified>					<!-- Relaxe NPTG alpha [prefix to allow 1   -->				</Date>				<Description>Tariff Zone types for National Public Transport XML schemas.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.naptan.org.uk/schema/2.5/napt}NaPT_TariffZone-v2-5.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.naptan.org.uk/schema/2.5/napt/NaPT_stop-v2-5.xsd</Requires>					<Requires>http://www.naptan.org.uk/schema/2.5/napt/NaPT_modes-v2-5.xsd</Requires>				</Relation>				<Rights>Unclassified                             <Copyright>Crown Copyright 2000-2013</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.5</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Air transport, Airports, Ports and maritime transport, Ferries (marine), Public transport, Bus services, Coach services, Bus stops and stations, Rail transport, Railway stations and track, Train services, Underground trains, Business and industry, Transport, Air transport , Ports and maritime transport, Public transport, Rail transport, Roads and road transport </Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Tariff Zone types for National Public Transport XML schemas.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>NaPT_TariffZone    HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<xsd:include schemaLocation="NaPT_stop-v2-5.xsd"/>	<xsd:include schemaLocation="NaPT_modes-v2-5.xsd"/>	<!-- ======================================================================= -->	<!-- ===Typess==================================================== -->	<xsd:simpleType name="TariffZoneTypeEnumeration">		<xsd:annotation>			<xsd:documentation>Allowed values for classifying NaPTAN TARIFF ZONEs (groups).</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="GPBS">				<xsd:annotation>					<xsd:documentation>Paired on-street Bus / Coach / Tram stops.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="pairedOnStreetBusStops"/>			<xsd:enumeration value="GCLS">				<xsd:annotation>					<xsd:documentation>Clustered on-street Bus / Coach / Tram stops.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="clusteredOnStreetBusStops"/>			<xsd:enumeration value="GAIR">				<xsd:annotation>					<xsd:documentation>Airport Building.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="airportBuilding"/>			<xsd:enumeration value="GBCS">				<xsd:annotation>					<xsd:documentation>Bus / Coach Station.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="busOrCoachStation"/>			<xsd:enumeration value="GFTD">				<xsd:annotation>					<xsd:documentation>Ferry Terminal / Dock Building.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ferryTerminalOrDockBuilding"/>			<xsd:enumeration value="GTMU">				<xsd:annotation>					<xsd:documentation>Tram / Metro / Underground Station.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="tramMetroOrUndergroundStation"/>			<xsd:enumeration value="GRLS">				<xsd:annotation>					<xsd:documentation>Railway station.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="railwayStation"/>			<xsd:enumeration value="GMLT">				<xsd:annotation>					<xsd:documentation>Multimode Interchange.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="multimodaInterchange"/>			<xsd:enumeration value="GOTH">				<xsd:annotation>					<xsd:documentation>Other Structure.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="otherStructure"/>			<xsd:enumeration value="GCCH">				<xsd:annotation>					<xsd:documentation>Coach Service Coverage.</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="coachCoverage"/>			<xsd:enumeration value="GLCB">				<xsd:annotation>					<xsd:documentation> Lift or Cablecar station (+NaPT v2.4)</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="liftOrCableCarStation"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ====Entity Identifiers==================================================== -->	<xsd:simpleType name="NetworkCodeType">		<xsd:annotation>			<xsd:documentation>The ATCO recommended coding for TARIFF ZONEs (groups) as defined in the NaPTAN specification.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string"/>	</xsd:simpleType>	<xsd:complexType name="NetworksStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more Networks or zone schems.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="Network" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="Network" substitutionGroup="VersionedObjectTimestamped">		<xsd:annotation>			<xsd:documentation>A grouping transport services marketed as a single brand. or fare scheme. (+NaPT V2.5)</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="NetworkStructure">					<xsd:sequence>						<xsd:element ref="Extensions" minOccurs="0"/>					</xsd:sequence>				</xsd:extension>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="NetworkStructure">		<xsd:annotation>			<xsd:documentation>A group or area of related stops.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectTimestampedStructure">				<xsd:sequence>					<xsd:element name="NetworkCode" type="NetworkCodeType">						<xsd:annotation>							<xsd:documentation>Code that uniquely identifies the NETWORK within the UK.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Name" type="NaturalLanguagePlaceNameStructure">						<xsd:annotation>							<xsd:documentation>Name of the NETWORK</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="ShortName" type="NaturalLanguagePlaceNameStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Short Name of the NETWORK</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Modes" type="VehicleModesList" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Modes of  Network.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="AdministrativeAreaRef" type="AdministrativeAreaRefStructure">						<xsd:annotation>							<xsd:documentation>NPTG administrative area that manages NETWORK data. </xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="TariffZones" type="TariffZonesStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>TARIFF ZONEs in Network</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="NetworkRef" type="NetworkRefStructure"/>	<xsd:complexType name="NetworkRefStructure">		<xsd:annotation>			<xsd:documentation>Data type for a direct reference to a NETWORK that is defined elsewhere using a NetworkStructure. The reference is by area code.</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="NetworkCodeType"/>		</xsd:simpleContent>	</xsd:complexType>	<!-- ====TariffZone========================================================= -->	<xsd:simpleType name="TariffZoneCodeType">		<xsd:annotation>			<xsd:documentation>The ATCO recommended coding for TARIFF ZONEs (groups) as defined in the NaPTAN specification.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string"/>	</xsd:simpleType>	<xsd:complexType name="TariffZonesStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more TARIFF ZONEs.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="TariffZone" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>A Fare Zone comprising one or more STOP POINTs. (+NaPT V2.5)</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="TariffZone" type="TariffZoneStructure" substitutionGroup="VersionedObjectTimestamped">		<xsd:annotation>			<xsd:documentation>A Fare Zone within a fare scheme (+NaPT v2.5)  </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="TariffZoneStructure">		<xsd:annotation>			<xsd:documentation>A group or area of related stops.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedObjectTimestampedStructure">				<xsd:sequence>					<xsd:element name="TariffZoneCode" type="TariffZoneCodeType">						<xsd:annotation>							<xsd:documentation>Code for TARIFF ZONE.  The Network code is nromallused as a prefix, e.g. TFL:ZONE1</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="Name" type="NaturalLanguagePlaceNameStructure">						<xsd:annotation>							<xsd:documentation>Name of the TARIFF ZONE, e.g. Zone 1.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="ShortName" type="NaturalLanguagePlaceNameStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Long Name of the TARIFF ZONE</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ====Refs======================================================== -->	<xsd:complexType name="TariffZoneRefStructure">		<xsd:annotation>			<xsd:documentation>Data type for a direct reference to a TARIFF ZONE that is defined elsewhere using a TariffZoneStructure. The reference is by area code.</xsd:documentation>		</xsd:annotation>		<xsd:simpleContent>			<xsd:extension base="TariffZoneCodeType"/>		</xsd:simpleContent>	</xsd:complexType>	<xsd:complexType name="TariffZoneRefsStructure">		<xsd:annotation>			<xsd:documentation>A collection of referneces to one or more TARIFF ZONEs.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element name="TariffZoneRef" type="TariffZoneRefStructure" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>A Fare Zone comprising one or more STOP POINTs</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ====Place========================================================= --></xsd:schema>