<?xml version="1.0" encoding="UTF-8"?><!-- (c) Crown Copyright 2000-2007   --><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.transxchange.org.uk/" xmlns:core="http://www.govtalk.gov.uk/core" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.5" id="TXC_journey_support">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Contributor>Revised for version 2.3 Kizoom Limited, by  Nicholas Knowles. mailto:schemer@kizoom.com</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.4 Nick Knowles, Kizoom mailto:schemer@kizoom.com</Creator>				<Date>					<Created>2010-30-01</Created>				</Date>				<Date>					<Modified>2010-01-31</Modified>					<!-- 2.4  Modularised from txc_common							  -->				</Date>				<Description>Common journey  types for National Public Transport XML schemas. Includes Dates, Times, Date and Time ranges. Weekday day types and  public holiday day types.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.5/txc/}TXC_journey_support-v2-5.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.w3.org/2001/xml.xsd</Requires>					<Requires>http://www.naptan.org.uk/schema/2.5/napt/NaPT_stop-v2-5.xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2013</Copyright>				</Rights>				<Source>Factored out from TXC 2.0 and JW 3.0 draft schemas</Source>				<Status>Version 2.5</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport,Public transport, Bus services, Coach services, Bus stops and stationsRail transport, Infrastructure (railways),  Train services,Roads and road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common journey types for National Public Transport XML schemas.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_ journey_support   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="../napt/NaPT_stop-v2-5.xsd"/>	<xsd:simpleType name="TimeDemandEnumeration">		<xsd:annotation>			<xsd:documentation>Classifications of a vehicle journey's "time demand" which affects running/waiting times.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="earlyMorning"/>			<xsd:enumeration value="peakMorning"/>			<xsd:enumeration value="offPeak"/>			<xsd:enumeration value="peakAfternoon"/>			<xsd:enumeration value="evening"/>			<xsd:enumeration value="lateEvening"/>			<xsd:enumeration value="saturdayMorning"/>			<xsd:enumeration value="saturdayDaytime"/>			<xsd:enumeration value="saturdayEvening"/>			<xsd:enumeration value="sunday"/>			<xsd:enumeration value="bankHoliday"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="JourneyStopTypeEnumeration">		<xsd:annotation>			<xsd:documentation>Allowed values for use of stop in journey.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="origin"/>			<xsd:enumeration value="intermediate"/>			<xsd:enumeration value="destination"/>			<xsd:enumeration value="originDestination"/>		</xsd:restriction>	</xsd:simpleType>	<!-- ======================================================================= -->	<!--====Directions===============================================================-->	<xsd:simpleType name="ServiceDirectionEnumeration">		<xsd:annotation>			<xsd:documentation>Directions on service</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="inbound"/>			<xsd:enumeration value="outbound"/>			<xsd:enumeration value="inboundAndOutbound"/>			<xsd:enumeration value="circular"/>			<xsd:enumeration value="clockwise"/>			<xsd:enumeration value="antiClockwise"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="JourneyPatternVehicleDirectionEnumeration">		<xsd:annotation>			<xsd:documentation>Directions on journey pattern</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="inherit">				<xsd:annotation>					<xsd:documentation>Value is inherited from parent</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="inbound"/>			<xsd:enumeration value="outbound"/>			<xsd:enumeration value="inboundAndOutbound"/>			<xsd:enumeration value="circular"/>			<xsd:enumeration value="clockwise"/>			<xsd:enumeration value="antiClockwise"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="LinkDirectionEnumeration">		<xsd:annotation>			<xsd:documentation>Direction of Link.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="inbound"/>			<xsd:enumeration value="outbound"/>			<xsd:enumeration value="clockwise"/>			<xsd:enumeration value="antiClockwise"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="UnrestrictedDirectionType">		<xsd:annotation>			<xsd:documentation>Direction taken (for example, eastbound, inbound, anti-clockwise).</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string"/>	</xsd:simpleType>	<!--====Arrival/Departure======================================================-->	<!-- ===Interchange====================================================================== -->	<xsd:simpleType name="VehicleAtStopActivityEnumeration">		<xsd:annotation>			<xsd:documentation>Activity undertaken by vehicle at a stop.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:NMTOKEN">			<xsd:enumeration value="pickUp"/>			<xsd:enumeration value="setDown"/>			<xsd:enumeration value="pickUpAndSetDown"/>			<xsd:enumeration value="pass"/>		</xsd:restriction>	</xsd:simpleType>	<!--====Instructions===============================================================-->	<!--==== Stop  times element groups============================ --></xsd:schema>