<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:apd="http://www.govtalk.gov.uk/people/AddressAndPersonalDetails" xmlns="http://www.transxchange.org.uk/" xmlns:napt="http://www.naptan.org.uk/" targetNamespace="http://www.transxchange.org.uk/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.5" id="TXC_accessibilityBooking">	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Contributor>Revised for version 2.4 Kizoom Limited, by Nicholas Knowles. mailto:schemer@transxchage.org.uk</Contributor>				<Coverage>United Kingdom</Coverage>				<Creator>Created for TXC 2.5 Nick Knowles   mailto:schemer@transxchage.org.uk</Creator>				<Date>					<Created>2013-03-31</Created>				</Date>				<Date>					<Modified>2013-03-31</Modified>					<!-- 2.5  Modularised from txc_common					   PTIC-087 Based on NeTEx booking model  							  -->				</Date>				<Description> Common Accessibility Booking  element declarations for TransXChange, an XML schema for exchanging bus schedules.</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.transxchange.org.uk/schema/2.5/txc/}TXC_accessibilityBookingv2-5.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Department for Transport, Great Minster House, 76 Marsham Street, London SW1P 4DR</Publisher>				<Relation>					<Requires>http://www.transxchange.org.uk/schema/2.5/napt/NaPT_modes-v2-5.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.5/napt/NaPT_utility_types-v2-5.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.5/napt/NPTG_administrative_support-v2-4.xsd</Requires>					<Requires>http://www.transxchange.org.uk/schema/2.5/napt/NaPT_stopAccessibility-v2-5..xsd</Requires>				</Relation>				<Rights>Unclassified                               <Copyright>Crown Copyright 2000-2013</Copyright>				</Rights>				<Source>Factored out from 2.0 draft schema</Source>				<Status>Version 2.4</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport, Public transport, Bus services, Coach services, Bus stops and stations Rail transport, Infrastructure (railways), Train Bus stops and stations, road transport</Category>					<Project>Transport Direct</Project>				</Subject>				<Title>Common Accessibility Booking   Elements for TransXChange, an XML format for exchanging bus timetables.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>TXC_ garage   HEADER DOCUMENTATION</xsd:documentation>	</xsd:annotation>	<!-- ======================================================================= -->	<xsd:include schemaLocation="../napt/NaPT_utility_types-v2-5.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_modes-v2-5.xsd"/>	<xsd:include schemaLocation="../nptg/NPTG_administrative_support-v2-4.xsd"/>	<xsd:include schemaLocation="../napt/NaPT_stopAccessibility-v2-5.xsd"/>	<!-- ======================================================================= -->	<xsd:complexType name="AccessibilityBookingsStructure">		<xsd:annotation>			<xsd:documentation>A collection of one or more Accessibility Bookings</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DeltaListStructure">				<xsd:sequence>					<xsd:element ref="AccessibilityBooking" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="AccessibilityBooking" type="AccessibilityBookingStructure">		<xsd:annotation>			<xsd:documentation>Accessibility booking details for A transport operator.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="AccessibilityBookingStructure">		<xsd:annotation>			<xsd:documentation>Type for Accessibility booking details for A transport operator.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedChildStructure">				<xsd:sequence>					<xsd:element name="Mode" type="VehicleModesEnumeration" minOccurs="0"/>					<xsd:element name="Regions" type="RegionRefsStructure" minOccurs="0"/>					<xsd:element name="AssistanceAvailability" type="AssistanceAvailabilityEnumeration" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Availability of Assistance services</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:element name="WheelchairBookingRequired" type="xsd:boolean" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Whetehr wheelchair booking is available or required.</xsd:documentation>						</xsd:annotation>					</xsd:element>					<xsd:group ref="BookingContactGroup"/>					<xsd:group ref="BookingArrangementsGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="BookingContactGroup">		<xsd:annotation>			<xsd:documentation>Elements for  Booking Contact</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="ContactTelephoneNumber" type="TelephoneContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact telephone number for accessibility booking.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="EmailAddress" type="EmailAddressType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The email address of the operator. It is up to the operator whether an individual's address, or a generic company e-mail address is used.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="BookingArrangementsGroup">		<xsd:sequence>			<xsd:element name="BookingMethod" type="BookingMethodListOfEnumerations" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Methods of booking allowed.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="MinimumBookingPeriod" type="xsd:duration" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Minimum interval in advance of departure day or time that service may  be ordered.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LatestBookingTime" type="xsd:time" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Latest time in tehe day that a booking can be made.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="BookingUrl" type="xsd:anyURI" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The web site address for accessibility infro and or booking</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Note" type="NaturalLanguageStringStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Additional note on booking arrangements</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= --></xsd:schema>