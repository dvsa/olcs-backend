<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TransXChangePublisherTransport xsi:schemaLocation="http://www.transxchange.org.uk/publisher http://www.transxchange.org.uk/schema/2.1/publisher/3.1.2/TransXChangePublisherService.xsd" xmlns="http://www.transxchange.org.uk/publisher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <PublishDocumentRequest>
        <InputOperands>
            <DocumentPath></DocumentPath>
            <DocumentName></DocumentName>
        </InputOperands>
        <OutputOperands>
            <OutputPath></OutputPath>
        </OutputOperands>
        <ProcessingOptions>
            <ValidateXml>false</ValidateXml>
            <StopData>webService</StopData>
            <MapData>webService</MapData>
        </ProcessingOptions>
        <OutputOptions>
            <Sections>
                <Particulars>basic</Particulars>
                <EmbeddedParticulars>true</EmbeddedParticulars>
                <Timetable>full</Timetable>
                <RouteTrack>full</RouteTrack>
                <Diagnostics>full</Diagnostics>
            </Sections>
        </OutputOptions>
        <Filters>
            <TimingPointTypes>PTP</TimingPointTypes>
            <MergeFrequencyJourneys>true</MergeFrequencyJourneys>
        </Filters>
        <RouteTrackMapOptions>
            <RouteScale>auto</RouteScale>
            <RouteGrouping>byDirection</RouteGrouping>
            <RouteTiling>A4</RouteTiling>
            <StopListOrder>map</StopListOrder>
        </RouteTrackMapOptions>
        <Rendering>
            <OutputFormat>pdf</OutputFormat>
            <Background>official</Background>
        </Rendering>
    </PublishDocumentRequest>
</TransXChangePublisherTransport>
