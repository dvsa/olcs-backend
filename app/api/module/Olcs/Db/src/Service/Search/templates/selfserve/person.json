{
   "query": {
      "bool": {
         "filter": [
            {
               "bool": {
                  "must_not": [
                     {
                        "terms": {
                           "lic_status": [
                              "lsts_not_submitted",
                              "lsts_unlicenced",
                              "lsts_withdrawn"
                           ]
                        }
                     },
                     {
                        "terms": {
                           "contact_type": [
                              "ct_complainant"
                           ]
                        }
                     },
                     {
                        "terms": {
                           "tm_status_id": [
                              "tm_s_rem"
                           ]
                        }
                     },
                     {
                        "terms": {
                           "found_as": [
                              "Historical TM"
                           ]
                        }
                     },
                     {
                        "exists": {
                           "field": "date_removed"
                        }
                     },
                     {
                        "terms": {
                           "contact_type": [
                              "ct_obj"
                           ]
                        }
                     },
                     {
                        "exists": {
                           "field": "is_deleted"
                        }
                     }
                  ]
               }
            }
         ],
         "must": [
            {
               "exists": {
                  "field": "lic_no"
               }
            }
         ],
         "should": [
            {
               "multi_match": {
                  "query": "%SEARCH_TERM%",
                  "fuzziness": 2,
                  "prefix_length": 1,
                  "fields": [
                     "person_fullname"
                  ]
               }
            },
            {
               "multi_match": {
                  "query": "%SEARCH_TERM%",
                  "boost": 2,
                  "fields": [
                     "person_fullname"
                  ]
               }
            },
            {
               "match": {
                  "lic_no": "%SEARCH_TERM%"
               }
            },
            {
               "match": {
                  "tm_id": "%SEARCH_TERM%"
               }
            }
         ]
      }
   },
   "min_score": 7
}
