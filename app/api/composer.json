{
    "name": "olcs/backend",
    "description": "Back End Application for OLCS Project",
    "repositories": [
        {
            "url": "git@gitlab.clb.npm:olcs/olcs-logging.git",
            "type": "vcs"
        },
        {
            "type": "package",
            "package": {
                "name": "olcs/companies-house",
                "version": "dev-develop",
                "source": {
                    "url": "git@gitlab.clb.npm:olcs/companies-house.git",
                    "type": "git",
                    "reference": "origin/develop"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/MajorCaiger/phpcov.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.clb.npm:olcs/olcs-autoload.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "~2.3.3",
        "doctrine/orm": "~2.4.0",
        "doctrine/doctrine-orm-module": "0.*",
        "doctrine/annotations": "1.2.1",
        "gedmo/doctrine-extensions": "~2.3.9",
        "olcs/companies-house": "dev-develop",
        "ruflin/Elastica": "dev-master",
        "olcs/olcs-logging": "dev-develop",
        "olcs/autoload": "dev-develop"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5.3",
        "phpunit/phpunit": "4.*",
        "sebastian/diff": "dev-master",
        "phpunit/phpcov": "dev-master",
        "mockery/mockery": "~0.9.2"
    },
    "autoload": {
        "classmap": [
            "module/Olcs/Db/src",
            "vendor/olcs/companies-house/lib"
        ],
        "psr-4": {
            "Olcs\\Db\\": "module/Olcs/Db/src",
            "CompaniesHouse\\": "vendor/olcs/companies-house/lib"
        }
    },
    "config": {
        "preferred-install": "dist",
        "github-oauth": {
            "github.com": "9d28c36d14775b5d8c4a5ffeee6172eecbd8c9f4"
        }
    }
}
