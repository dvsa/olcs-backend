{
    "name": "olcs/backend",
    "description": "Back End Application for OLCS Project",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://satis.inf.mgt.mtpdvsa",
            "type": "composer"
        },
        {
            "url": "git@gitlab.inf.mgt.mtpdvsa:olcs/olcs-logging.git",
            "type": "vcs"
        },
        {
            "url": "git@gitlab.inf.mgt.mtpdvsa:olcs/olcs-transfer.git",
            "type": "vcs"
        },
        {
            "url": "git@gitlab.inf.mgt.mtpdvsa:olcs/companies-house.git",
            "type": "vcs"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.inf.mgt.mtpdvsa:olcs/olcs-autoload.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.3.3",
        "doctrine/orm": "~2.5.0",
        "doctrine/doctrine-orm-module": "~0.8.0",
        "doctrine/annotations": "^1.2.3",
        "gedmo/doctrine-extensions": "~2.3.9",
        "ruflin/Elastica": "v1.4.3.0",
        "olcs/companies-house": "dev-develop",
        "olcs/olcs-transfer": "dev-develop",
        "olcs/olcs-logging": "dev-develop",
        "olcs/autoload": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "mockery/mockery": "~0.9.3"
    },
    "autoload": {
        "classmap": [
            "module/Olcs/Db/src",
            "vendor/olcs/companies-house/lib"
        ],
        "psr-4": {
            "Olcs\\Db\\": "module/Olcs/Db/src",
            "Dvsa\\Olcs\\Api\\": "module/Api/src",
            "CompaniesHouse\\": "vendor/olcs/companies-house/lib"
        }
    },
    "config": {
        "preferred-install": "dist",
        "github-oauth": {
            "github.com": "9d28c36d14775b5d8c4a5ffeee6172eecbd8c9f4"
        }
    }
}