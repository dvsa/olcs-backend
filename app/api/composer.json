{
    "name": "olcs/backend",
    "description": "Back End Application for OLCS Project",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/OLCS",
            "type": "composer"
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/Packagist-Proxy",
            "type": "composer"
        },
        {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/league",
            "type": "composer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "aws/aws-sdk-php": "^3.99",
        "beberlei/doctrineextensions": "^1.0",
        "doctrine/annotations": "^1.2.3",
        "doctrine/collections": "~1.4.0",
        "doctrine/doctrine-orm-module": "~0.10.0",
        "doctrine/orm": "~2.6.3",
        "doctrine/persistence": "~1.3.4",
        "fzaninotto/faker": "^1.6",
        "gedmo/doctrine-extensions": "~2.3.12",
        "league/flysystem": "^1.0",
        "league/flysystem-webdav": "2.0.0",
        "olcs/olcs-companies-house": "~4.206.0",
        "olcs/olcs-cpms": "~4.206.0",
        "olcs/olcs-logging": "~4.206.0",
        "olcs/olcs-transfer": "~4.206.0",
        "olcs/olcs-utils": "~4.206.0",
        "olcs/olcs-xmltools": "~4.206.0",
        "oro/doctrine-extensions": "^1.2",
        "phprtflite/phprtflite": "~1.3.3",
        "phpseclib/phpseclib": "^2.0.6",
        "qandidate/toggle": "^1.1",
        "ramsey/uuid": "^3.6",
        "ruflin/elastica": "~6.1",
        "simplesamlphp/saml2": "~3.4.2",
        "soflomo/purifier": "^1.0",
        "symfony/filesystem": "~3.0.0",
        "symfony/finder": "~3.2",
        "twig/twig": "^2.0",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-barcode": "^2.5",
        "zendframework/zend-cache": "^2.5",
        "zendframework/zend-captcha": "^2.5",
        "zendframework/zend-code": "^2.5",
        "zendframework/zend-config": "^2.5",
        "zendframework/zend-console": "^2.5",
        "zendframework/zend-crypt": "^2.5",
        "zendframework/zend-db": "^2.5",
        "zendframework/zend-debug": "^2.5",
        "zendframework/zend-di": "^2.5",
        "zendframework/zend-dom": "^2.5",
        "zendframework/zend-escaper": "^2.5",
        "zendframework/zend-eventmanager": "^2.5",
        "zendframework/zend-feed": "^2.5",
        "zendframework/zend-file": "^2.5",
        "zendframework/zend-filter": "^2.5",
        "zendframework/zend-form": "^2.5",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-i18n": "^2.5",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-inputfilter": "^2.5",
        "zendframework/zend-json": "^2.5",
        "zendframework/zend-loader": "^2.5",
        "zendframework/zend-log": "^2.5",
        "zendframework/zend-mail": "^2.5",
        "zendframework/zend-math": "^2.5",
        "zendframework/zend-memory": "^2.5",
        "zendframework/zend-mime": "^2.5",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-mvc": "^2.5",
        "zendframework/zend-navigation": "^2.5",
        "zendframework/zend-paginator": "^2.5",
        "zendframework/zend-permissions-acl": "^2.5",
        "zendframework/zend-permissions-rbac": "^2.5",
        "zendframework/zend-progressbar": "^2.5",
        "zendframework/zend-serializer": "^2.5",
        "zendframework/zend-server": "^2.5",
        "zendframework/zend-servicemanager": "^2.5",
        "zendframework/zend-session": "^2.5",
        "zendframework/zend-soap": "^2.5",
        "zendframework/zend-stdlib": "^2.5",
        "zendframework/zend-tag": "^2.5",
        "zendframework/zend-text": "^2.5",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-validator": "^2.5",
        "zendframework/zend-view": "^2.5",
        "zendframework/zend-xmlrpc": "^2.5",
        "zendframework/zendxml": "^1.0.1",
        "zf-commons/zfc-rbac": "~2.5.0"
    },
    "require-dev": {
        "olcs/olcs-devtools": "~4.206.0",
        "mikey179/vfsstream": "~1.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-mockery": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "mockery/mockery": "1.2.4",
        "johnkary/phpunit-speedtrap": "^3.0"
    },
    "autoload": {
        "classmap": [
            "module/Olcs/Db/src"
        ],
        "psr-4": {
            "Olcs\\Db\\": "module/Olcs/Db/src",
            "Dvsa\\Olcs\\Api\\": "module/Api/src",
            "Dvsa\\Olcs\\DocumentShare\\": "module/DocumentShare/src",
            "Dvsa\\Olcs\\Cli\\": "module/Cli/src",
            "Dvsa\\Olcs\\Scanning\\": "module/Scanning/src",
            "Dvsa\\Olcs\\Email\\": "module/Email/src",
            "Dvsa\\Olcs\\Address\\": "module/Address/src",
            "Dvsa\\Olcs\\Snapshot\\": "module/Snapshot/src",
            "Dvsa\\Olcs\\GdsVerify\\": "module/GdsVerify/src",
            "Dvsa\\Olcs\\Queue\\": "module/Queue/src",
            "Dvsa\\Olcs\\AwsSdk\\": "module/AwsSdk/src"
        }
    },
    "config": {
        "process-timeout": 600,
        "preferred-install": "dist",
        "secure-http": false,
        "platform": {
            "php": "7.1.16"
        },
        "sort-packages": true
    },
    "scripts": {
        "ut": "phpunit -c test/phpunit.xml --stop-on-failure",
        "review": "review",
        "cs": "phpcs --severity=1 --standard=vendor/olcs/coding-standards/Profiles/DVSA/CS/ruleset.xml",
        "coverage": "phpunit -c test/phpunit.xml --coverage-html ./test/coverage/html",
        "sonar": "phpunit -c test/phpunit.xml --coverage-clover ./test/coverage/coverage-report.clover --log-junit ./test/coverage/log.junit",
        "update-syms": "composer-update-syms",
        "release": "release"
    },
    "extra": {
        "nexus-push": {
            "url": "http://nexus.shd.ci.nonprod.dvsa.aws/repository/olcs-backend",
            "ignore": [
                "test/",
                "config/autoload/local.php"
            ]
        }
    }
}
